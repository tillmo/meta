    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#adjoint -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#adjoint">
        <rdfs:comment>The subject is a translation that shares its sentence and model translation with the object reduction. The signature translations are adjoint to each other (in the sense of category theory).</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Reduction"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Translation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#alignmentIn -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#alignmentIn">
        <rdfs:comment>The obbject is a DOL library, in which the subject is an alignment between two participating OMS.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#compositionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#compositionOf">
        <rdfs:comment>Developers&apos; note: The subject mapping is the composition of the object list of mappings (list item 1 ∘ list item 2 ∘ … ∘ list item n).  All elements of the latter list must be instances of the same class as the subject.  (Enforcing the latter constraint goes beyond OWL.)</rdfs:comment>
        <rdfs:comment>Range can&apos;t be specified in OWL DL</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#conservativelyExtendsTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#conservativelyExtendsTo">
        <rdfs:label xml:lang="en">conservatively extends to</rdfs:label>
        <rdfs:comment>The object is a conservative extension of the subject</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ConservativeExtensionMapping"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#extendsTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#definitionallyExtendsTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#definitionallyExtendsTo">
        <rdfs:label>definitionally extends to</rdfs:label>
        <rdfs:comment>the object is a definitional extension of the subject</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DefinitionalExtensionMapping"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#conservativelyExtendsTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#embeddableInto -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#embeddableInto">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>The subject is embeddable into the object logic.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#exactlyMappableTo"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableToBijectivelyOnModels"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableToLogic"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#translatableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#exactLogicalExpressivity -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#exactLogicalExpressivity">
        <rdfs:comment>The subject language has exactly the expressivity of the object logic.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#exactlyMappableTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#exactlyMappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is an exact mapping from the subject to the object logic.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#weaklyExactlyMappableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#extendsTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#extendsTo">
        <rdfs:label>extends to</rdfs:label>
        <rdfs:comment>The subject is a mapping of an extension, the object is the target of the extension mapping, i.e. the extended OMS.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ExtensionMapping"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#faithfullyMappableTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#faithfullyMappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a faithful mapping from the subject to the object logic.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableToLogic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#formalizes -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#formalizes">
        <rdfs:label>formalizes</rdfs:label>
        <rdfs:comment>The subject is a precise mathematical entity capturing the object (which is an informal or semi-formal entity)</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isFormalizedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#hasAlignment -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#hasAlignment">
        <rdfs:comment>The subject is a DOL library, in which the object is an alignment between two participating OMS.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Alignment"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#alignmentIn"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasMapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#hasHeterogeneousEnvironment -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#hasHeterogeneousEnvironment">
        <rdfs:label>has logical environment</rdfs:label>
        <skos:definition>relationship between the subject and a heterogeneous environment in which it is embedded or is associated with.</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#HeterogeneousEnvironment"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#hasInterpretation -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#hasInterpretation">
        <rdfs:comment>The subject is a DOL library, in which the object is an interpretation between two participating OMS.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Interpretation"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasMapping"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#interpretationIn"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#hasLanguageAspect -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#hasLanguageAspect">
        <rdfs:label>has language aspect</rdfs:label>
        <skos:definition>the relation between an OMS language and its language aspects</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LanguageAspect"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#hasMapping -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#hasMapping">
        <rdfs:comment>The subject is a DOL library, in which the object is a mapping between two participating OMS.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasMember"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#hasMember -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#hasMember">
        <skos:definition>The inverse of the set membership relation.</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLLibrary"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#memberOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#hasOMS -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#hasOMS">
        <rdfs:comment>The subject is a DOL library, in which the object is a participating heterogeneous OMS.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasMember"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#hasPart -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#hasPart">
        <rdfs:label>has part</rdfs:label>
        <skos:definition>relationship between two things were the second is a component of the first</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#hasSubset -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#hasSubset">
        <rdfs:label>has subset</rdfs:label>
        <skos:definition>relationship between two sets where every member of the second set belongs to the first set</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Set"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Set"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#impliedExtendsTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#impliedExtendsTo">
        <rdfs:label>implied extends to</rdfs:label>
        <skos:definition>the relation of a model-theoretic conservative extension that does not introduce new non-logical symbols</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ImpliedExtensionMapping"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#definitionallyExtendsTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#impliedExtensionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#impliedExtensionOf"/>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#induces -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#induces"/>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#interpretationIn -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#interpretationIn">
        <rdfs:comment>The object is a DOL library, in which the subject is an interpretation between two participating OMS.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isAdjointOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isAdjointOf">
        <rdfs:label>is adjoint of</rdfs:label>
        <rdfs:comment>The subject is a translation that shares its sentence and model translation with the object reduction. The signature translations are adjoint to each other (in the sense of category theory).</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isApproximantOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isApproximantOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is approximant of</rdfs:label>
        <skos:definition>approximation (in the sense of a logically implied theory, possibly aftersuitable translation) of an OMS in a smaller signature or OMS language</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isCombinationOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isCombinationOf">
        <rdfs:label>is combination of OMS</rdfs:label>
        <skos:example>Consider an ontology involving a concept Person, and another one involving Human being, and an alignment that relates these to concepts. In the combination of the ontologies along the alignment, there is only one concept, representing both Person and Human being.</skos:example>
        <skos:definition>aggregation of all the OMS in an OMS network, where non-logical symbols are shared according to the OMS mappings in the OMS network </skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Combination"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSNetwork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isConsequenceTheoreticallyConservativeExtensionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isConsequenceTheoreticallyConservativeExtensionOf">
        <rdfs:comment>The subject OMS is a consequence-theoretic conservative extension of the object OMS. A consequence-theoretic conservative extension is an extension that does not add new theorems (in terms of the unextended signature).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isConservativeExtensionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isConservativeExtensionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isConservativeExtensionOf">
        <rdfs:label>is conservative extension of</rdfs:label>
        <rdfs:comment>the subject is a conservative extension of the object</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isExtensionOf"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isModuleOf"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isMappedBy"/>
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#conservativelyExtendsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isDefinitionalExtensionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isDefinitionalExtensionOf">
        <rdfs:label>is definitional extension of</rdfs:label>
        <skos:note> An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$. </skos:note>
        <rdfs:comment>Developers&apos; note: We need FOL, or the same reification approach as chosen for the different kinds of translations, for capturing that an extension is definitional if and only if it is weakly definitional and model-theoretically conservative.</rdfs:comment>
        <skos:definition>extension whose newly introduced non-logical symbols are interpreted in a unique way</skos:definition>
        <skos:note> Each definitional extension is also a monomorphic extension but not vice versa. </skos:note>
        <skos:note> $O_2$ being a definitional extension of $O_1$ implies a bijective correspondence between the classes of models of $O_2$ and $O_1$. </skos:note>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isModelTheoreticallyConservativeExtensionOf"/>
        <strictlyStrongerThan rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isMonomorphicExtensionOf"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isMonomorphicExtensionOf"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isWeakDefinitionalExtensionOf"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isMappedBy"/>
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#definitionallyExtendsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isElementOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isElementOf">
        <rdfs:label>is element of OMS</rdfs:label>
        <skos:definition>any resource in an OMS (e.g. a non-logical symbol, a sentence, a correspondence, the OMS itself, ...) or a named set of such resources.</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isExtensionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isExtensionOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <skos:prefLabel>extends</skos:prefLabel>
        <skos:altLabel>is extension of</skos:altLabel>
        <rdfs:comment>Relation between two OMS. The subject OMS is an extension of the object OMS, i.e. the sets of non-logical symbols and sentences of the subject OMS are supersets of those of the object OMS.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isMappableTo"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isSubOMSOf"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isMappedBy"/>
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#extendsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isFormalizedBy -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isFormalizedBy">
        <rdfs:label>is formalized by</rdfs:label>
        <rdfs:comment>The object is a precise mathematical entity capturing the subject (which is an informal or semi-formal entity)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isImpliedExtensionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isImpliedExtensionOf">
        <rdfs:label>is implied extension of</rdfs:label>
        <skos:note>Each implied extension is also a definitional extension but not vice versa.</skos:note>
        <skos:note> A conservative extension $O_2$ of an OMS $O_1$ is an implied extension, if and only if the signature of $O_2$ is the signature of $O_1$. $O_2$ is an implied extension of $O_1$ if and only if the model class of $O_2$ is the model class of $O_1$.</skos:note>
        <skos:definition>model-theoretic conservative extension that does not introduce new non-logical symbols</skos:definition>
        <strictlyStrongerThan rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isConservativeExtensionOf"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isDefinitionalExtensionOf"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isMappedBy"/>
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#impliedExtendsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isLanguageAspectOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isLanguageAspectOf">
        <rdfs:label>is language aspect of</rdfs:label>
        <skos:definition>a language asecpt is a set of language constructs of a given language, not necessarily forming a sublanguage</skos:definition>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasLanguageAspect"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isLogicallyInteroperableWith -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isLogicallyInteroperableWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:label>is logically interoperable with</rdfs:label>
        <skos:definition>property of structured OMS, which may be written in different OMS languages supporting different logics, of being usable jointly in a coherent way (via suitable OMS language translations), such that the notions of their overall consistency and logical entailment have a precise logical semantics</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#StructuredOMS"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#StructuredOMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isMappableTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isMappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is mappable to</rdfs:label>
        <skos:editorialNote>reification of this property, and its subproperties, is illustrated by property chain</skos:editorialNote>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isMappedBy"/>
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isMappedBy -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isMappedBy">
        <rdfs:label>is mapped by</rdfs:label>
        <rdfs:comment>The object mapping maps from the subject into something of the same type as the subject.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isModelOfOMS -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isModelOfOMS">
        <rdfs:label>is a model of the OMS</rdfs:label>
        <skos:definition>relationship between a model and an OMS where the model is a member of the model class of the OMS (which for flattenable OMS is equivalent to being a model of the signature of the OMS and satisfying all sentences in the OMS).</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Model"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isModelOfSignature -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isModelOfSignature">
        <rdfs:label>is a model of the signature</rdfs:label>
        <skos:definition>relationship between a model and a signature (vocabulary) such that the model provides a semantic interpretation of all non-logical symbols in the signature</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Model"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Signature"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isModelTheoreticallyConservativeExtensionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isModelTheoreticallyConservativeExtensionOf">
        <rdfs:comment>The subject OMS is a model-theoretic conservative extension of the object OMS. A model-theoretic conservative extension is an extension that does not lead to a restriction of class of models of an OMS.</rdfs:comment>
        <strictlyStrongerThan rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isConsequenceTheoreticallyConservativeExtensionOf"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isConservativeExtensionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isModuleOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isModuleOf">
        <rdfs:label>is module of</rdfs:label>
        <skos:definition>relationship between two OMS where the first conservatively extends to the second OMS</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isSubOMSOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isMonomorphicExtensionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isMonomorphicExtensionOf">
        <rdfs:comment>The subject OMS is a monomorphic extension of the object OMS. A monomorphic extension is an extension whose newly introduced non-logical symbols are interpreted in a way unique up to isomorphism.</rdfs:comment>
        <strictlyStrongerThan rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isModelTheoreticallyConservativeExtensionOf"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isModelTheoreticallyConservativeExtensionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isOMSDocumentationOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isOMSDocumentationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label>is OMS documentation of</rdfs:label>
        <skos:definition>relationship between an OMS documentation and the documented OMS</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSDocumentation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isOMSUnionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isOMSUnionOf">
        <rdfs:label>is union of OMS</rdfs:label>
        <skos:definition>aggregation of several OMS to a new OMS, without any renaming</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#CollectionOfOMS"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isProfileOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isProfileOf">
        <rdfs:label>is profile of</rdfs:label>
        <skos:definition>see Class &apos;profile&apos;</skos:definition>
        <rdfs:comment>The subject is a isProfile of the object language.  This is a stronger notion than just being a sublanguage.  Supported serializations are shared along this property.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Profile"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isSubLanguageOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isSubLanguageOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isSubLanguageOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is sublanguage of</rdfs:label>
        <skos:definition>syntactically specified subset of a given language, consisting of a subset of its terminal and nonterminal symbols and grammar rules</skos:definition>
        <rdfs:comment>The subject is a sublanguage of the object language.  Supported logics are shared along the inverse of this property.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Language"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Language"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableToLanguage"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#translatableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isSubLogicOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isSubLogicOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is sublogic of</rdfs:label>
        <skos:definition>a logic that is a syntactic restriction of another logic, inheriting its semantics</skos:definition>
        <rdfs:comment>The subject is a sublogic of the object.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#embeddableInto"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#exactLogicalExpressivity"/>
            </rdf:Description>
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isSubLanguageOf"/>
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#exactLogicalExpressivity"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isSubOMSOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isSubOMSOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>is subOMS of</rdfs:label>
        <skos:definition>OMS whose sets of non-logical symbols and sentences are subsets of those present in a given larger OMS</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#isWeakDefinitionalExtensionOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#isWeakDefinitionalExtensionOf">
        <rdfs:comment>The subject OMS is a weak definitional extension of the object OMS. A weak definitional extension is an extension whose newly introduced
   non-logical symbols can be interpreted in at most one way.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#language -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#language">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <skos:definition>the language of an OMS</skos:definition>
        <rdfs:comment>Developers&apos; note: The reasoners in Protégé don&apos;t like us to say the following.  Together with the SubPropertyChain specified for supportsSerialization it would go beyond OWL DL anyway.
SubPropertyChain: serialization o serializes</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#logic -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#logic">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>the logic in which an OMS is formalized</rdfs:comment>
        <skos:definition>specification of valid reasoning that comprises signatures (user defined vocabularies), models (interpretations of these), sentences (constraints on models), and a satisfaction relation between models and sentences.</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mappableFrom -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mappableFrom">
        <rdfs:comment>There is a mapping from the object to the subject.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mappableFromLanguage -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mappableFromLanguage">
        <rdfs:comment>There is a mapping from the object language to the subject language.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mappableFromLogic -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mappableFromLogic">
        <rdfs:comment>There is a mapping from the object logic to the subject logic.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mappableTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a mapping from the subject to the object.</rdfs:comment>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#sourceOfMapping"/>
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mappableToBijectivelyOnModels -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mappableToBijectivelyOnModels">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a model-bijective mapping from the subject to the object logic.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#modelExpansivelyMappableTo"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#weaklyExactlyMappableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mappableToLanguage -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mappableToLanguage">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a mapping from the subject language to the object language.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableFromLanguage"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mappableToLogic -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mappableToLogic">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a mapping from the subject logic to the object logic.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableFromLogic"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mappingIn -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mappingIn">
        <rdfs:comment>The object is a DOL library, in which the subject is a mapping between two participating OMS.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasMapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mapsFrom -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mapsFrom">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label>maps from</rdfs:label>
        <skos:definition>relationship between a mapping and its domain</skos:definition>
        <rdfs:comment>The subject mapping maps from the object into something of the same type as the object.</rdfs:comment>
        <rdfs:comment>Developers&apos; note: enforcing the constraint that a mapping must map into something of the same type goes beyond OWL.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Mapping"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isMappedBy"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#sourceOfMapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mapsTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mapsTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label>maps to</rdfs:label>
        <rdfs:comment>The subject mapping maps from something into the object (which is of the same type).  Enforcing the latter constraint goes beyond OWL.</rdfs:comment>
        <skos:definition>relationship between a mapping and its codomain</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Mapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#memberOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#memberOf">
        <skos:definition>The set membership relation.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#modelExpansivelyMappableTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#modelExpansivelyMappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a model-expansive mapping from the subject to the object logic.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#faithfullyMappableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#omsIn -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#omsIn">
        <rdfs:comment>The subject is an OMS participating in a DOL library, which is the object.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasOMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#plainlyMappableTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#plainlyMappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a plain mapping from the subject to the object logic.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#simplyTheoroidallyMappableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#reducibleFrom -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#reducibleFrom">
        <rdfs:comment>There is a reduction from the object to the subject.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableFrom"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#reducibleTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#reducibleTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a reduction from the subject to the object.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableTo"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#reducibleFrom"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#satisfies -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#satisfies">
        <rdfs:label>satisfies</rdfs:label>
        <terms:abstract></terms:abstract>
        <skos:definition>relationship between a model and a sentence indicating that the sentence holds true in the model</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Model"/>
        <rdfs:seeAlso rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SatisfactionRelation"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Sentence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#serialization -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#serialization">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>the textual syntax in which an OMS is written</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Serialization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#serializes -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#serializes">
        <rdfs:label>serializes</rdfs:label>
        <rdfs:comment>The subject is a serialization for the object language. For example, Manchester syntax serializes OWL</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Serialization"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsSerialization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#share -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#share">
        <skos:definition>property of OMS symbols being mapped to the same symbol when computing a combination of an OMS network.</skos:definition>
        <skos:note>Sharing is always relative to a given OMS network that
  relates different OMS. That is, two given OMS symbols
  can share with respect to one OMS network, and not share with respect to some
  other OMS network.</skos:note>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#NonLogicalSymbol"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#NonLogicalSymbol"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#simplyTheoroidallyMappableTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#simplyTheoroidallyMappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a simple theoroidal mapping from the subject to the object logic.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableToLogic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#source -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#source">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <skos:definition>the source OMS of a mapping</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#sourceOfMapping -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#sourceOfMapping">
        <rdfs:comment>The object mapping maps from the subject into something of the same type as the subject.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#specifiesSemanticsOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#specifiesSemanticsOf">
        <rdfs:comment>The semantics of [a subset of] the object language can be specified in terms of the subject logic.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#supportsLogic -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic">
        <rdfs:label>supports logic</rdfs:label>
        <skos:definition>logic represented by a satisfaction relation of the language.</skos:definition>
        <rdfs:comment>The semantics of [a subset of] the subject language can be specified in terms of the object logic.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isSubLogicOf"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isSubLanguageOf"/>
            </rdf:Description>
            <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#supportsLogicMapping -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#supportsLogicMapping">
        <rdfs:comment>The subject language mapping corresponds to the object logic mapping.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguageTranslation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#supportsSerialization -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#supportsSerialization">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment>Note that the serialization should be as specific as possible, i.e. one should not say that ``OWL can be serialized in XML&apos;&apos; and ``Common Logic can be serialized in XML&apos;&apos;, but instead ``OWL can be serialized in OWL XML&apos;&apos; and ``Common Logic can be serialized in XCL&apos;&apos;, taking into account that OWL XML and XCL are two different XML languages.</rdfs:comment>
        <rdfs:comment>OMS in the subject language can be serialized in the object serialization.</rdfs:comment>
        <rdfs:comment>Developers&apos; note: The reasoners in Protégé don&apos;t like us to say the following.  Together with the SubPropertyChain specified for language it would go beyond OWL DL anyway.
SubPropertyChain: isProfileOf o supportsSerialization</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Serialization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#target -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#target">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <skos:definition>the target OMS of a mapping</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#translatableFrom -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#translatableFrom">
        <rdfs:comment>There is a translation from the object to the subject.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableFrom"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#translatableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#translatableTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#translatableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a translation from the subject to the object.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableTo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#usesLanguage -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#usesLanguage">
        <rdfs:label>uses language</rdfs:label>
        <skos:definition>relation between something and a language used by one or more components of it.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#weaklyExactlyMappableTo -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#weaklyExactlyMappableTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>There is a weakly exact mapping from the subject to the object logic.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableToLogic"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#altFilenameExtension -->

    <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#altFilenameExtension">
        <skos:definition>an alternative filename extension for a serialization</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Serialization"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema-datatypes#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#mediaType -->

    <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#mediaType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>the media type (MIME type) of a serialization</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Serialization"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema-datatypes#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#prefFilenameExtension -->

    <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/DOL/Current/ontology#prefFilenameExtension">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <skos:definition>the preferred filename extension for a serialization</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Serialization"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema-datatypes#string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#AbstractSyntax -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#AbstractSyntax">
        <rdfs:label>abstract syntax</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
        <skos:definition>term language for representing documents in a machine-processable way</skos:definition>
        <skos:note>An abstract syntax can be specified as a MOF metamodel. Then abstract abstract syntax documents can be represented as  XMI documents.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Alignment -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Alignment">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Correspondence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Interpretation"/>
        <skos:definition>an OMS mapping expressing a collection of relations between entities of the two OMS</skos:definition>
        <rdfs:comment>an alignment between two heterogeneous OMS in a DOL library</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#AnnotationExpression -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#AnnotationExpression">
        <rdfs:label>annotation expression</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Expression"/>
        <skos:note> According to note~\ref{note:annotation-rdf} it is possible to interpret annotations under an RDF semantics. ``Without a logical semantics&apos;&apos; in this definition means that annotations to an OMS are not considered sentences of that OMS. </skos:note>
        <skos:note>\label{note:annotation-rdf} Formally, an annotation is given as a $(\text{subject}, \text{predicate}, \text{object})$ triple as defined by \nisref{SOURCE: W3C/TR REC-rdf-concepts:2004, Section 6}. The subject of an annotation is an element of an OMS. The predicate is an RDF property defined in an external OMS and describes in what way the annotation object is related to the annotation subject. </skos:note>
        <skos:definition>additional information without a logical semantics that is attached to an element of an OMS</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#AnnotationExpressionLanguageAspect -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#AnnotationExpressionLanguageAspect">
        <rdfs:label>annotation expression language aspect</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LanguageAspect"/>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicalLanguageAspect"/>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#StructuringLanguageAspect"/>
        <skos:definition>the (unique) language aspect of an OMS language that enables the expression of comments and annotations</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#AnswerSubstitution -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#AnswerSubstitution">
        <rdfs:label>answer substitution</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Substitution"/>
        <skos:definition>substitution that, when applied to a given query, turns the latter into a logical consequence of a given OMS</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Approximant -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Approximant">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>approximation (in the sense of a logically implied theory, possibly after suitable translation)  of an OMS in a smaller signature or a sublanguage.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Approximation -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Approximation">
        <rdfs:label>approximation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>OMS mapping to a logically implied theory (possibly after suitable translation) in a smaller signature or OMS language</skos:definition>
        <skos:editorialNote>this definition is derived from the defintion of approximant in the spec</skos:editorialNote>
        <skos:definition>structured OMS that delivers an approximation.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#AxiomSentence -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#AxiomSentence">
        <rdfs:label>axiom sentence</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Sentence"/>
        <skos:definition>sentence postulated to be valid (i.e.\ true in every model)</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#BasicOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#BasicOMS">
        <rdfs:label>basic OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#FlattenableOMS"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#NativeOMS"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#StructuredOMS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSDocumentation"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#induces"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Signature"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#induces"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SetOfSentences"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>a basic OMS (in a single language, logic, and serialization)</rdfs:comment>
        <skos:definition>signature equipped with a set of sentences and annotations, which may be used as a building block for a larger OMS</skos:definition>
        <skos:note> The sentences must use only those non-logical symbols that are present in the signature. </skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Category -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Category">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <skos:note>The morphisms determine which part of the structure of the objects is relevant, i.e. preserved by morphisms. Hence, objects can be seen as &apos;sets with structure&apos;, and morphisms as &apos;structure-preserving maps&apos;.  However note that not all categories can be obtained in this way.</skos:note>
        <skos:note>In DOL, objects of a category are usually signatures or OMS, and morphisms are signature morphisms, or OMS mappings. In principle, no assumption about the exact nature of objects and morphisms is made.</skos:note>
        <skos:definition>a collection of objects with suitable morphisms between them</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Closure -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Closure">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note>DOL supports four different forms of closure: minimization and maximization as well as freeness and cofreeness.</skos:note>
        <skos:definition>way of implementing the closed world assumption by restricting the models to those that are minimal or maximal (with respect to the local environment). The closed world assumption is a default assumption about facts whose status in unknown.</skos:definition>
        <skos:note>Symbols from the local environment are assumed to have a fixed interpretation. Only the symbols newly declared in the closure are forced to have minimal or maximal interpretation.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Cofreeness -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Cofreeness">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Closure"/>
        <skos:definition>special type of closure, restriction of models to those that are cofree (with respect to the local environment).</skos:definition>
        <skos:note>In first-order-like logics, cofreeness means maximal interpretation of predicates and equality being observable equivalence. Cofreeness can be used for the specification of coinductive datatypes like infinite lists and streams.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#CollectionOfCorrespondences -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#CollectionOfCorrespondences">
        <rdfs:label>collection of correspondences</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLEntity"/>
        <skos:note>collection is used here in the sense of syntactic collection</skos:note>
        <skos:definition>collection whose members are correspondences.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#CollectionOfExpressions -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#CollectionOfExpressions">
        <rdfs:label>collection of expressions</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
        <skos:definition>collection whose members are expressions</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#CollectionOfOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#CollectionOfOMS">
        <rdfs:label>collection of OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLEntity"/>
        <skos:definition>collection whose members are OMS.</skos:definition>
        <skos:note>collection is used here in the sense of syntactic collection</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Combination -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Combination">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSNetwork"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:example>Consider an ontology involving a concept Person, and another one involving Human being, and an alignment that relates these to concepts. In the combination of the ontologies along the alignment, there is only one concept, representing both Person and Human being.</skos:example>
        <skos:definition>aggregation of all the OMS in an OMS network, where non-logical symbols are shared according to the OMS mappings in the OMS network </skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ConsequenceTheoreticConservativeExtension -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ConsequenceTheoreticConservativeExtension">
        <rdfs:label>consequence-theoretic conservative extension</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ConservativeExtension"/>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a consequence-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$.</skos:note>
        <skos:definition>extension that does not add new theorems (in terms of the unextended signature).</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ConsequenceTheoreticConservativeExtensionMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ConsequenceTheoreticConservativeExtensionMapping">
        <rdfs:label>consequence-theoretic conservative extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ConservativeExtensionMapping"/>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a consequence-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$.</skos:note>
        <skos:definition>extension that does not add new theorems (in terms of the unextended signature)</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isConsequenceTheoreticallyConservativeExtensionOf"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ConservativeExtension -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ConservativeExtension">
        <rdfs:label>conservative extension</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Extension"/>
        <skos:definition>extension that does not add new logical properties with respect to the signature of the extended OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ConservativeExtensionMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ConservativeExtensionMapping">
        <rdfs:label>conservative extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ExtensionMapping"/>
        <skos:note>If used without qualification, the consequence-theoretic version is meant.</skos:note>
        <skos:definition>extension mapping of a consequence-theoretic or model-theoretic conservative extension</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isConservativeExtensionOf"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ConsistentOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ConsistentOMS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#InconsistentOMS"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>OMS that has a non-trivial set of logical consequences in the sense that not every sentence follows from the OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Correspondence -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Correspondence">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLEntity"/>
        <skos:note>A correspondence is given as a quadruple 
$(e_1, R, e_2, c)$,
where $R$ denotes the type of relationship that is asserted to hold between the two non-logical 
symbols/terms, and 0&lt;=c&lt;=1 is a confidence value.  $R$ and $c$ may be omitted: When $R$ is 
omitted, it defaults to the equivalence relation, unless another default
relation has been explicitly specified; when $c$ is omitted, it defaults to 1.</skos:note>
        <skos:definition>relationship between an non-logical symbol $e_1$ from an OMS $O_1$ and an non-logical symbol $e_2$ from an OMS $O_2$, or between an non-logical symbol $e_1$ from $O_1$ and a term $t_2$ formed from non-logical symbols\index{non-logical symbol from $O_2$.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#DOLDocument -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#DOLDocument">
        <rdfs:label>DOL document</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLEntity"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Document"/>
        <skos:definition>document containing a DOL library.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#DOLEntity -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#DOLEntity">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#DOLLibrary -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#DOLLibrary">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLEntity"/>
        <skos:note>collection is used here in the sense of syntactic collection</skos:note>
        <skos:definition>collection of named OMS and OMS networks, possibly written in different OMS languages, linked by named OMS mappings.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS">
        <rdfs:label>DOL structured OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#StructuredOMS"/>
        <skos:definition>syntactically valid DOL expression denoting an OMS that is built from smaller OMS as building blocks.</skos:definition>
        <skos:note>DOL structured OMS, typically, use basic OMS as building blocks for defining other structured OMS, OMS mappings or OMS networks.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#DefaultMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#DefaultMapping">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <skos:definition>logic mapping that will be chosen by default if only the source and target logic are given</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#DefinitionalExtension -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#DefinitionalExtension">
        <rdfs:label>definitional extension</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ModelTheoreticConservativeExtension"/>
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#WeakDefinitionalExtension"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MonomorphicExtension"/>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$.</skos:note>
        <skos:definition>extension whose newly introduced non-logical symbols are interpreted in a unique way.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#DefinitionalExtensionMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#DefinitionalExtensionMapping">
        <rdfs:label>definitional extension mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ModelTheoreticConservativeExtensionMapping"/>
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#WeakDefinitionalExtensionMapping"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MonomorphicExtensionMapping"/>
        <skos:note>$O_2$ being a definitional extension of $O_1$ implies a bijective correspondence between the classes of models of $O_2$ and $O_1$.</skos:note>
        <skos:definition>extension whose newly introduced non-logical symbols are interpreted in a unique way</skos:definition>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$.</skos:note>
        <skos:note>Each definitional extension is also a monomorphic extension but not vice versa.</skos:note>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isDefinitionalExtensionOf"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Document -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Document">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
        <skos:definition>result of serializing an OMS or DOL library using a given serialization.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ElusiveOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ElusiveOMS">
        <rdfs:label>elusive OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#FlattenableOMS"/>
        <skos:definition>OMS that is not flattenable</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Embedding -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Embedding">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ExactMapping"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ModelBijectiveMapping"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Translation"/>
        <skos:definition>logic mapping that embeds the source into the target logic, using components that are embeddings and (in the case of model translations) isomorphism</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#embeddableInto"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Entailment -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Entailment">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <skos:note>Entailments can also be stated between networks, or networks and OMS.</skos:note>
        <skos:note>Entailment expresses that each model satisfying the first OMS also satisfies the second OMS (or the sentence, respectively).</skos:note>
        <skos:definition>relation between two OMS (or an OMS and a sentence) expressing that the second one is logically implied by the first one.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Equivalence -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Equivalence">
        <rdfs:label>equivalence</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <skos:definition>OMS mapping ensuring that two OMS share the same definable concepts</skos:definition>
        <skos:note> Two OMS are equivalent if they have a common definitional extension. The OMS may be written in different OMS languages. </skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ExactMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ExactMapping">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#WeaklyExactMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <skos:definition>logic mapping that is compatible with certain DOL structuring constructs</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#exactlyMappableTo"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Expression -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Expression">
        <rdfs:label>expression</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#usesLanguage"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Language"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>a finite combination of symbols that are well-formed according to applicable rules (depending on the language)</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Extension -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Extension">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>OMS whose sets of non-logical symbols and sentences are supersets of those present in the OMS given by the local environment.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ExtensionMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ExtensionMapping">
        <rdfs:label>extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <skos:definition>mapping between two OMS where the sets of non-logical symbols and sentences of the second OMS are supersets of those present in the first OMS</skos:definition>
        <skos:editorialNote>This definition differs from that in the Terms and Definition section of the DOL specification by focussing on the mapping rather than the codomain of the mapping.</skos:editorialNote>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#FaithfulMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#FaithfulMapping">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <skos:definition>logic mapping that preserves and reflects logical consequence</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#faithfullyMappableTo"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Filtering -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Filtering">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>verbatim removal of symbols or axioms from an OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#FlattenableOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#FlattenableOMS">
        <rdfs:label>flattenable OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>OMS that can be seen, by purely syntactical means, to be logically equivalent to a basic OMS</skos:definition>
        <skos:note>More precisely, an OMS is flattenable if and only if it is either a basic OMS or it is an extension, union, translation, module extraction, approximation, filtering, or reference of named OMS involving only flattenable OMS.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#FormalSemantics -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#FormalSemantics">
        <rdfs:label>formal semantics</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SemanticDomain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#formalizes"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Language"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SemanticRule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>assignment of a mathematical meaning to a language by mapping abstract syntax to suitable semantic domains</skos:definition>
        <skos:note>A formal semantics is a formalization of the meaning of a language.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Freeness -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Freeness">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Closure"/>
        <skos:definition>special type of closure, restriction of models to those that are free (with respect to the local environment).</skos:definition>
        <skos:note>In first-order logic (and similar logics), freeness means minimal interpretation of predicates and minimal equality among data values. Freeness can be used for the specification of inductive datatypes like numbers, lists, trees, bags etc. In order to specify e.g. lists over some elements, the specification of the elements should be in the local environment.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#GlobalEnvironment -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#GlobalEnvironment">
        <rdfs:label>global environment</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LocalEnvironment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>mapping from identifiers (IRIs) to values in  semantics domains representing the global knowledge about OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Graph -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Graph">
        <rdfs:label>graph</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <skos:definition>set of objects (nodes) that are connected by links (edges)</skos:definition>
        <skos:altLabel>network</skos:altLabel>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#HeterogeneousEnvironment -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#HeterogeneousEnvironment">
        <rdfs:label>heterogeneous environment</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicGraph"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LanguageGraph"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SupportsRelation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>environment for the expression of homogeneous and heterogeneous OMS, comprising a logic graph, an OMS language graph and a supports relation</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#HeterogeneousOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#HeterogeneousOMS">
        <rdfs:label>heterogeneous OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#HomogeneousOMS"/>
        <skos:definition>OMS whose parts are supported by different logics</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#HomogeneousOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#HomogeneousOMS">
        <rdfs:label>homogeneous OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>OMS whose parts are supported by one logic</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ImpliedExtension -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ImpliedExtension">
        <rdfs:label>implied extension</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DefinitionalExtension"/>
        <skos:definition>model-theoretic conservative extension that does not introduce new non-logical symbols.</skos:definition>
        <skos:note>A conservative extension $O_2$ of an OMS
    $O_1$ is an implied extension, if and only if the signature of
    $O_2$ is the signature of $O_1$.  $O_2$ is an implied extension of
    $O_1$ if and only if the model class of $O_2$ is the model class
    of $O_1$.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ImpliedExtensionMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ImpliedExtensionMapping">
        <rdfs:label>implied extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DefinitionalExtensionMapping"/>
        <skos:definition>model-theoretic conservative extension that does not introduce new non-logical symbols</skos:definition>
        <skos:note>Each implied extension is also a definitional extension but not vice versa.</skos:note>
        <skos:note>A conservative extension $O_2$ of an OMS $O_1$ is an implied extension, if and only if the signature of $O_2$ is the signature of $O_1$. $O_2$ is an implied extension of $O_1$ if and only if the model class of $O_2$ is the model class of $O_1$.</skos:note>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#impliedExtensionOf"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Import -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Import">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <skos:definition>reference to an OMS behaving as if it were verbatim included; also import of DOL libraries.</skos:definition>
        <skos:example>An owl:import in OWL is an import.</skos:example>
        <skos:note>The purpose of $O_2$ importing $O_1$ is to make non-logical symbols and sentences of $O_1$ available in $O_2$.</skos:note>
        <skos:note>The import of a whole DOL library into another DOL library is also called import.</skos:note>
        <skos:note>Semantically, an import of $O_2$ into $O_1$ is equivalent to the verbatim inclusion of $O_2$ in place of the import declaration.</skos:note>
        <skos:note>Importing $O_1$ into $O_2$ turns $O_2$ into an extension of $O_1$.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#InconsistentOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#InconsistentOMS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ConsistentOMS"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#UnsatisfiableOMS"/>
        <skos:definition>OMS that has a trivial set of logical consequences in the sense that every sentence follows from the OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Institution -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Institution">
        <rdfs:label>institution</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Signature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SatisfactionRelation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Category"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SignatureMorphism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#formalizes"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Sentence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Model"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>metaframework mathematically formalising the notion of a logic</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#InterfaceSignature -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#InterfaceSignature">
        <rdfs:label>interface signature</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Signature"/>
        <skos:definition>signature mediating between an OMS and a module of that OMS in the sense that it contains those non-logical symbols that the sentences of the module and the sentences of the OMS have in common.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Interpretation -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Interpretation">
        <owl:equivalentClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#View"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <skos:definition>OMS mapping that postulates a specialization relation between two OMS along a morphism between their signatures</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Language -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Language">
        <rdfs:label>language</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#CollectionOfExpressions"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>collection of expressions</skos:definition>
        <skos:note>collection is used here in the sense of syntactic collection</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LanguageAspect -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LanguageAspect">
        <rdfs:label>language aspect</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
        <skos:definition>set of language constructs of a given language, not necessarily forming a sublanguage</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LanguageGraph -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LanguageGraph">
        <rdfs:label>OMS language graph</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Graph"/>
        <skos:definition>graph of OMS languages and OMS language translations, typically used in a heterogeneous environment.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LanguageMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LanguageMapping">
        <rdfs:label>language mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Language"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Language"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Mapping"/>
        <skos:definition>mapping between languages</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LinkedData -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LinkedData">
        <rdfs:label>linked data</rdfs:label>
        <skos:definition>structured data that is published on the Web in a machine-processable way, according to principles specified in \cite{BernersLee:LinkedData2006,BizerHeath09}}</skos:definition>
        <skos:note> Using HTTP content negotiation \cite{rfc2616} it is possible to serve representations in different formats from the same URL. </skos:note>
        <skos:note> The linked data principles (adapted from \cite{BernersLee:LinkedData2006} and its paraphrase at \cite{Wikipedia:LinkedData2011}) are the following: \begin{enumerate} \item Use IRIs as names for things. \item Use HTTP IRIs so that these things can be referred to and looked up (``dereferenced&apos;&apos;) by people and user agents.\footnote{I.e., the IRI is treated as a URL (uniform resource locator).} \item Provide useful machine-processable (plus optionally human-readable) information about the thing when its IRI is dereferenced, using standard formats. \item Include links to other, related IRIs in the exposed data to improve discovery of other related information on the Web. \end{enumerate} </skos:note>
        <skos:note> RDF, serialized as RDF/XML \cite{W3C:REC-rdf-syntax-grammar-20040210}, is the most common format for publishing linked data. However, its usage is not mandatory. </skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LocalEnvironment -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LocalEnvironment">
        <rdfs:label>local environment</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>OMS built from all previously-declared symbols and axioms.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Logic -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Logic">
        <rdfs:label>logic</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isFormalizedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Institution"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note> Most OMS languages have an underlying logic. </skos:note>
        <skos:definition>specification of valid reasoning that comprises signatures, sentences, models, and a satisfaction relation between models and sentences</skos:definition>
        <skos:note> See annex~\ref{a:rdf-logic-vocab} for the organization of the relation between OMS languages and their logics and serializations. </skos:note>
        <skos:example> $\mathcal{SROIQ}(D)$ is the logic underlying OWL 2 DL. </skos:example>
        <rdfs:comment>a logic that defines the semantics of an language</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LogicGraph -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LogicGraph">
        <rdfs:label>OMS logic graph</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Graph"/>
        <skos:definition>graph of logics, logic translations and logic reductions, typically used in a heterogeneous environment.</skos:definition>
        <skos:note>In a logic graph, some of the logic translations and reductions can be marked to be default translations.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LogicMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping">
        <rdfs:label>logic mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Mapping"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguageTranslation"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <rdfs:comment>a mapping (translation or reduction) between two logics</rdfs:comment>
        <skos:definition>mapping between logics</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LogicReduction -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LogicReduction">
        <rdfs:label>logic reduction</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Reduction"/>
        <skos:definition>reduction of a source logic onto a (usually less expressive) target logic (mapping signatures, sentences and models) that simply forgets those parts of the logical structure not fitting the target logic</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LogicTranslation -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LogicTranslation">
        <rdfs:label>logic translation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <skos:definition>translation of a source logic into a target logic (mapping signatures, sentences and models) that keeps or encodes the logical content of OMS</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LogicalLanguageAspect -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LogicalLanguageAspect">
        <rdfs:label>logical language aspect</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LanguageAspect"/>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#StructuringLanguageAspect"/>
        <skos:definition>the (unique) language aspect of an OMS language that enables the expression of non-logical symbols and sentences in a logical language</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#LogicalTheory -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LogicalTheory">
        <rdfs:label>logical theory</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SetOfSentences"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Signature"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>signature equipped with a set of sentences over the signature</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#MDEModel -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#MDEModel">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>logical theory that is used as a model in the sense of model-driven engineering (MDE)</skos:definition>
        <skos:note>Not to be confused with the term \termref{model} in the sense
of logic (model theory).</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Mapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Mapping">
        <rdfs:label>mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsFrom"/>
                        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
                        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <skos:note>in some cases is a morphism, as in Category Theory.</skos:note>
        <skos:note>alternative term: function</skos:note>
        <rdfs:comment>a generic mapping (translation or reduction)</rdfs:comment>
        <skos:definition>relation between a set of inputs and a set of permissible outputs with the property that each input is related to exactly one output.</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableTo"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Matching -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Matching">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Tool"/>
        <skos:definition>algorithmic procedure that generates an alignment for two given OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Maximization -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Maximization">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Closure"/>
        <skos:definition>form of closure that restricts the models to those that are maximal (with respect to the local environment).</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#MaximumApproximant -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#MaximumApproximant">
        <rdfs:label>maximum approximant</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Approximant"/>
        <skos:note>Technically, a maximum approximant is a uniform interpolant</skos:note>
        <skos:definition>best possible (in the sense of a maximum set of logical consequences approximant of an OMS in a smaller signature or a sublanguage.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Minimization -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Minimization">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Closure"/>
        <skos:definition>form of closure that restricts the models to those that are minimal (with respect to the local environment).</skos:definition>
        <skos:note>Also known as circumscription.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Model -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Model">
        <rdfs:label>model</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isModelOfSignature"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Signature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note>A model of an OMS is a model of the signature of the OMS that also satisfies all the additional constraints expressed by the OMS. In case of flattenable OMS, these constraints are expressed by the axioms of the OMS.</skos:note>
        <skos:definition> semantic interpretation of all non-logical symbols of a signature</skos:definition>
        <skos:scopeNote>This term is not to be confused with model (here called MDEModel) in the sense of modeling (i.e., the ``M&apos;&apos; in OMS).</skos:scopeNote>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ModelBijectiveMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ModelBijectiveMapping">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ModelExpansiveMapping"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#WeaklyExactMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <skos:definition>logic mapping that has a bijective mapping of models</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableToBijectivelyOnModels"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ModelExpansiveMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ModelExpansiveMapping">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#FaithfulMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <skos:definition>logic mapping that has a surjective model translation (ensuring faithfulness of the mapping)</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#modelExpansivelyMappableTo"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ModelFinder -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ModelFinder">
        <rdfs:label>model finder</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Tool"/>
        <skos:definition>Tool that finds models of an OMS and thus proves it to be satisfiable</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ModelTheoreticConservativeExtension -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ModelTheoreticConservativeExtension">
        <rdfs:label>model-theoretic conservative extension</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ConservativeExtension"/>
        <skos:definition>extension that does not lead to a restriction of class of models of an OMS.</skos:definition>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a model-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ModelTheoreticConservativeExtensionMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ModelTheoreticConservativeExtensionMapping">
        <rdfs:label>model-theoretic conservative extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ConservativeExtensionMapping"/>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a model-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$.</skos:note>
        <skos:note>Any model-theoretic conservative extension is also a consequence-theoretic one.</skos:note>
        <skos:definition>extension mapping that does not lead to a restriction of class of models of an OMS</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isModelTheoreticallyConservativeExtensionOf"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Module -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Module">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#InterfaceSignature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>subOMS that conservatively extends to conservative extension the whole OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#ModuleExtraction -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ModuleExtraction">
        <rdfs:label>module extraction</rdfs:label>
        <skos:note>The goal of module extraction is ``decomposing an OMS into smaller, more manageable modules with appropriate dependencies&apos;&apos; \cite{DBLP:series/lncs/5445}</skos:note>
        <SpecificationMetadata:informativeReference>Cited and slightly adapted from \cite{SuarezFigueroaEtAl:OntologyGlossary2008}</SpecificationMetadata:informativeReference>
        <skos:definition>activity of obtaining from an OMS concrete modules to be used for a particular purpose (e.g. to contain a particular sub-signature of the original OMS)</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#MonomorphicExtension -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#MonomorphicExtension">
        <rdfs:label>monomorphic extension</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ModelTheoreticConservativeExtension"/>
        <skos:definition>extension whose newly introduced non-logical symbols are interpreted in a way unique up to isomorphism.</skos:definition>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a monomorphic extension, if each model of $O_1$ can be expanded to a model of $O_2$ that is unique up to isomorphism.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#MonomorphicExtensionMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#MonomorphicExtensionMapping">
        <rdfs:label>monomorphic extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ModelTheoreticConservativeExtensionMapping"/>
        <skos:note>Each monomorphic extension is also a model-theoretic conservative extension but not vice versa.</skos:note>
        <skos:definition>extension whose newly introduced non-logical symbols are interpreted in a way unique up to isomorphism</skos:definition>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a monomorphic extension, if each model of $O_1$ can be expanded to a model of $O_2$ that is unique up to isomorphism.</skos:note>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isMonomorphicExtensionOf"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#NativeDocument -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#NativeDocument">
        <rdfs:label>native document</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Document"/>
        <skos:definition>document containing a native OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#NativeOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#NativeOMS">
        <rdfs:label>native OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>collection of expressions (like non-logical symbols, sentences and structuring elements) from a given OMS language.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#NonLogicalSymbol -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#NonLogicalSymbol">
        <rdfs:label>non-logical symbol</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Term"/>
        <rdfs:comment>a non-logical symbol in an OMS</rdfs:comment>
        <skos:altLabel>OMS symbol</skos:altLabel>
        <skos:example>Non-logical symbols in OWL \nisref{W3C/TR REC-owl2-syntax:2009} (there called ``entities&apos;&apos;) comprise \begin{itemize} \item individuals (denoting objects from the domain of discourse), \item classes (denoting sets of objects; also called concepts), and \item properties (denoting binary relations over objects; also called roles). \end{itemize} This is opposed to logical symbols in OWL, e.g.\ those for intersection and union of classes. </skos:example>
        <skos:example> Non-logical symbols in Common Logic \nisref{ISO/IEC 24707:2007} comprise \begin{itemize} \item names (denoting objects from the domain of discourse), \item sequence markers (denoting sequences of objects). \end{itemize} This is opposed to logical symbols in Common Logic, e.g.\ logical connectives and quantifiers.</skos:example>
        <skos:note>The notion of ``atomic sentence&apos;&apos; used in logic is different, it usually may involve several non-logical symbols. </skos:note>
        <skos:definition>atomic expression or syntactic constituent of an OMS that requires an interpretation through a model</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#OMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMS">
        <rdfs:label>OMS (ontology, specification or model)</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#HeterogeneousOMS"/>
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#HomogeneousOMS"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#ElusiveOMS"/>
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#FlattenableOMS"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#usesLanguage"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note>collection is used here in the sense of syntactic collection</skos:note>
        <skos:definition>collection of expressions (like non-logical symbols, sentences and structuring elements) in a given OMS language (or several such languages)</skos:definition>
        <skos:definition>a basic or structured ontology, specification or MDE model</skos:definition>
        <skos:note>An OMS can be written in different OMS language serializations.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#OMSDocumentation -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMSDocumentation">
        <rdfs:label>OMS documentation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
        <SpecificationMetadata:informativeReference>Adapted from \cite{SuarezFigueroaEtAl:OntologyGlossary2008}</SpecificationMetadata:informativeReference>
        <skos:definition>set of all annotations to an OMS, plus any other documents and explanatory comments generated during the entire OMS building process</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage">
        <rdfs:label>OMS language</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Language"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic"/>
                        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsSerialization"/>
                        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Serialization"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic"/>
                        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsSerialization"/>
                        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Serialization"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <skos:note> An OMS language is used for the formal specification of OMS.</skos:note>
        <rdfs:comment>a language for OMS (ontologies, models or specifications)</rdfs:comment>
        <skos:example> OMS languages include OWL, Common Logic, F-logic, UML class diagrams, RDFS, and OBO. </skos:example>
        <skos:definition>language equipped with a formal, declarative, logic-based semantics, plus non-logical annotations</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#OMSLanguageTranslation -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguageTranslation">
        <rdfs:label>OMS language translation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LanguageMapping"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Mapping"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsFrom"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <skos:definition>mapping from constructs in the source OMS language to their equivalents in the target OMS language</skos:definition>
        <skos:definition>a mapping (translation or reduction) between two OMS languages</skos:definition>
        <skos:note> An OMS language translation shall satisfy the property that the result of a translation is a well-formed text in the target language. </skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#OMSMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping">
        <rdfs:label>OMS mapping</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsFrom"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mapsTo"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLEntity"/>
        <rdfs:comment>a mapping between two heterogeneous OMS in a DOL library</rdfs:comment>
        <skos:definition>relationship between two OMS, typically given as a set of correspondences</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#mappableTo"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#OMSNetwork -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMSNetwork">
        <rdfs:label>OMS network</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLEntity"/>
        <skos:definition>collection of named OMS, possibly written in different OMS languages, linked by named OMS mappings and named OMS networks</skos:definition>
        <skos:altLabel>hyperontology</skos:altLabel>
        <skos:note>collection is used here in the sense of syntactic collection</skos:note>
        <skos:note>An OMS network is a diagram of OMS in the sense of category theory, but different from a diagram in the sense of model-driven architecture.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#OMSReduction -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMSReduction">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Translation"/>
        <skos:definition>restriction of an OMS to a smaller signature.</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#reducibleTo"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Ontology -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Ontology">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>logical theory that is used as a shard conceptualization</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#PlainMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#PlainMapping">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SimpleTheoroidalMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <skos:definition>logic mapping that maps signatures to signatures and therefore does not use infrastructure axioms</skos:definition>
        <rdfs:comment>Developers&apos; note: Protégé 4.2 doesn&apos;t like &apos;Class: OMSMapping DisjointUnionOf: PlainMapping, SimpleTheoroidalMapping&apos;, so we do it this way:</rdfs:comment>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#plainlyMappableTo"/>
    </owl:Class>
    <owl:Axiom>
        <rdfs:comment>Developers&apos; note: Protégé 4.2 doesn&apos;t like &apos;Class: OMSMapping DisjointUnionOf: PlainMapping, SimpleTheoroidalMapping&apos;, so we do it this way:</rdfs:comment>
        <owl:annotatedSource rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#PlainMapping"/>
        <owl:annotatedTarget rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SimpleTheoroidalMapping"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2002/07/owl#disjointWith"/>
    </owl:Axiom>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Profile -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Profile">
        <rdfs:label>profile</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Logic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note> Profiles can have different logics, even with completely different semantics, e.g.\ OWL 2 DL versus OWL 2 Full.</skos:note>
        <skos:note>Profiles typically correspond to sublogics. </skos:note>
        <skos:note> The logic needs to support the language. </skos:note>
        <skos:definition>(syntactic) sublanguage of an OMS language interpreting according to a particular logic that targets specific applications or reasoning methods} </skos:definition>
        <skos:example>Profiles of OWL 2 include OWL 2 EL, OWL 2 QL, OWL 2 RL, OWL 2 DL, and OWL 2 Full.</skos:example>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Query -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Query">
        <rdfs:label>query</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Sentence"/>
        <skos:definition>sentence containing query variables that can be instantiated by a substitution</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#QueryLanguage -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#QueryLanguage">
        <rdfs:label>query language</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
        <skos:scopeNote> There are also general purpose OMS languages, which can express both OMS and quries.</skos:scopeNote>
        <skos:definition>OMS language specifically dedicated to queries</skos:definition>
        <skos:example> SPARQL, Prolog </skos:example>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#QueryVariable -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#QueryVariable">
        <rdfs:label>query variable</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#NonLogicalSymbol"/>
        <skos:definition>symbol that will be used in a query and a substitution</skos:definition>
        <skos:note>Usually, query variables are the free variables of a sentence, whereas there can be other (bound) variables.</skos:note>
        <skos:note>From an abstract point of view, query variables are just symbols; they are used in a way that they will be substituted using a substitution. Many OMS languages have special notations for (query) variables.</skos:note>
        <skos:note>If there are no variables in an OMS language, constants can be used as query variables.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Reduction -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Reduction">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Mapping"/>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Translation"/>
        <skos:definition>mapping between languages or logics forgetting parts of the structure, projection to a smaller language or logic</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#reducibleTo"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Refinement -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Refinement">
        <rdfs:label>refinement</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <skos:definition>OMS mapping that postulates a specialization relation between two OMS along a morphism between their signatures</skos:definition>
        <rdfs:comment>refinement</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Renaming -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Renaming">
        <rdfs:label>renaming</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note> A renaming results in an OMS mapping between the original and the renamed OMS. </skos:note>
        <skos:definition>assignment of new names to some non-logical symbols of an OMS</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Resource -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Resource">
        <rdfs:label>resource</rdfs:label>
        <skos:definition>something that can be globally identified</skos:definition>
        <skos:note>\nisref{IETF/RFC 3986:2005, Section 1.1} deliberately defines a resource as ``in a general sense \textelp{} whatever might be identified by \textins*{an IRI}&apos;&apos;. The original source refers to URIs, but DOL uses the compatible IRI standard \nisref{IETF/RFC 3987:2005} for identification.</skos:note>
        <skos:example>Familiar examples include an electronic document, an image, a source of information with a consistent purpose (e.g., ``today&apos;s weather report for Los Angeles&apos;&apos;), a service (e.g., an HTTP-to-SMS gateway), and a collection of other resources. A resource is not necessarily accessible via the Internet; e.g., human beings, corporations, and bound books in a library can also be resources. Likewise, abstract concepts can be resources, such as the operators and operands of a mathematical equation, the types of a relationship (e.g., ``parent&apos;&apos; or ``employee&apos;&apos;), or numeric values (e.g., zero, one, and infinity). \nisref{IETF/RFC 3986:2005, Section 1.1}</skos:example>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SatisfactionRelation -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SatisfactionRelation">
        <rdfs:label>satisfaction relation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <skos:definition>relation between models and sentences indicating which sentences hold true in the model</skos:definition>
        <rdfs:seeAlso rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#satisfies"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SatisfiableOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SatisfiableOMS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#UnsatisfiableOMS"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ConsistentOMS"/>
        <skos:definition>OMS that is satisfied by least one model.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SemanticDomain -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SemanticDomain">
        <rdfs:label>semantic domain</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <skos:definition>mathematically-defined set of values that can represent the intended meanings of language constructs.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SemanticRule -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SemanticRule">
        <rdfs:label>semantic rule</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <skos:definition>specification of a mapping from abstract syntax to a semantic domain.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Sentence -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Sentence">
        <rdfs:label>sentence</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Term"/>
        <skos:note>In a model, on the one hand, a sentence is always true or false. In an OMS, on the other hand, a sentence can have several logical statuses: it can be an axiom, if postulated to be true; a theorem, if proven from other axioms and theorems; a conjecture, if expecting to be proven from other axioms and theorems; or have another of many possible statuses.</skos:note>
        <rdfs:comment>a sentence in an OMS</rdfs:comment>
        <skos:note>It is quite common that sentences are required to be closed (i.e.\ have no free variables). However, this depends on the OMS language at hand.</skos:note>
        <skos:definition>term that is either true or false in a given model, i.e.\ which is assigned a truth value in this model.</skos:definition>
        <skos:note>A sentence can conform to one or more signatures (namely those signatures containing all non-logical symbols used in the sentence).</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Serialization -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Serialization">
        <rdfs:label>OMS serialization</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#serializes"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:example>OWL uses the term ``serialization&apos;&apos;; the following are standard OWL serializations: OWL functional-style syntax, OWL/XML, OWL Manchester syntax, plus any standard serialization of RDF (e.g.  RDF/XML, Turtle, \dots). However, RDF/XML is the only one tools are required to implement. </skos:example>
        <skos:example> Common Logic uses the term ``dialect&apos;&apos;; the following are standard Common Logic dialects: Common Logic Interchange Format (CLIF), Conceptual Graph Interchange Format (GCIF), eXtended Common Logic Markup Language (XCL).</skos:example>
        <skos:note>Serializations serve as standard formats for exchanging OMS between tools.</skos:note>
        <skos:definition>specific syntactic encoding of a given OMS language</skos:definition>
        <rdfs:comment>synonymous to &apos;concrete syntax&apos;</rdfs:comment>
        <rdfs:comment>a serialization of an language</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Set -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Set">
        <rdfs:label>set</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <skos:definition>set whose members have a certain type, as specified by the type parameter.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SetOfAnnotationExpressions -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SetOfAnnotationExpressions">
        <rdfs:label>set of annotation expressions</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#CollectionOfExpressions"/>
        <skos:definition>set whose members are annotation expressions.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SetOfMappings -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SetOfMappings">
        <rdfs:label>set of mappings</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Set"/>
        <skos:definition>set whose members are mappings.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SetOfNonlogicalSymbols -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SetOfNonlogicalSymbols">
        <rdfs:label>set of non-logical symbols</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#CollectionOfExpressions"/>
        <skos:definition>set whose members are non-logical symbols</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SetOfSentences -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SetOfSentences">
        <rdfs:label>set of sentences</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Set"/>
        <skos:definition>set whose members are sentences</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Signature -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Signature">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SetOfNonlogicalSymbols"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>set (or otherwise structured entity) of non-logical symbols of an OMS</skos:definition>
        <skos:note>The signature of a term is the set of all non-logical symbols occuring in the term. The signature of an OMS language is the set of all non-logical symbols possible in that language.</skos:note>
        <skos:altLabel>signature</skos:altLabel>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SignatureMorphism -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SignatureMorphism">
        <rdfs:label>signature morphism</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#MathematicalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SymbolMapItem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note>A list of symbol map items induces a signature morphism.</skos:note>
        <skos:definition>mapping between two signatures, preserving the structure of the source signature within the target signature</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SimpleTheoroidalMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SimpleTheoroidalMapping">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#PlainMapping"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <skos:definition>logic mapping that maps signatures to theories</skos:definition>
        <rdfs:comment>the resulting theory tpyically contains infrastructure axioms that are needed for expressing the logic mapping</rdfs:comment>
        <rdfs:comment>orthogonal to WeaklyExactMapping and FaithfulMapping</rdfs:comment>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#simplyTheoroidallyMappableTo"/>
    </owl:Class>
    <owl:Axiom>
        <rdfs:comment>Developers&apos; note: Protégé 4.2 doesn&apos;t like &apos;Class: OMSMapping DisjointUnionOf: PlainMapping, SimpleTheoroidalMapping&apos;, so we do it this way:</rdfs:comment>
        <owl:annotatedSource rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#PlainMapping"/>
        <owl:annotatedTarget rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SimpleTheoroidalMapping"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2002/07/owl#disjointWith"/>
    </owl:Axiom>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Specification -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Specification">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>logical theory that is used as a formal specification</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#StandoffMarkup -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#StandoffMarkup">
        <rdfs:label>standoff markup</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Language"/>
        <skos:definition>way of providing annotations to subjects in external resources, without embedding them into the original resource (here: OMS)</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#StructuredOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#StructuredOMS">
        <rdfs:label>structured OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>OMS that results from other OMS by import, union, combination, renaming or other structuring operations</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#StructuringLanguageAspect -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#StructuringLanguageAspect">
        <rdfs:label>structuring language aspect</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LanguageAspect"/>
        <skos:definition>the (unique) language aspect of an OMS language that covers structured OMS as well as the relations of basic OMS and structured OMS to each other, including, but not limited to imports, OMS mappings, conservative extensions, and the handling of prefixes for CURIEs</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SubLogic -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SubLogic">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Embedding"/>
        <rdfs:comment>logic embedding that is &apos;syntactic&apos; in the sense that signature and sentence translations are inclusions.</rdfs:comment>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isSubLogicOf"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Substitution -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Substitution">
        <rdfs:label>substitution</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <skos:definition>OMS mapping that maps query variables of one OMS to complex terms of another OMS</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SupportsRelation -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SupportsRelation">
        <rdfs:label>supports relation</rdfs:label>
        <skos:definition>relation between OMS languages and logics expressing the logical language aspect of the former, namely that the constructs of the former lead to a logical theory in the latter.</skos:definition>
        <skos:note>There is also a supports relation between OMS languages and serializations, and one between language mappings and logic mappings.</skos:note>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsLogic"/>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsLogicMapping"/>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#supportsSerialization"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#SymbolMapItem -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#SymbolMapItem">
        <rdfs:label>symbol map item</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LinguisticEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#NonLogicalSymbol"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:note>A symbol map item is given as s_1 |-&gt; s_2, where $s_1$ is a symbol from the source OMS and $s_2$ is a symbol from the target  of the OMS mapping.</skos:note>
        <skos:definition>pair of symbols of two OMS, indicating how a symbol from the first OMS is mapped by a signature morphism to a symbol of the second OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Term -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Term">
        <rdfs:label>term</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Expression"/>
        <skos:definition>syntactic expression either consisting of a single non-logical symbol or recursively composed of other terms (a.k.a. its subterms)</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#TheoremProver -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#TheoremProver">
        <rdfs:label>theorem prover</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Tool"/>
        <skos:definition>software tool that attempts to prove that a given sentence is the logical consequence of some given set of axioms and theorems.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#TheoremSentence -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#TheoremSentence">
        <rdfs:label>theorem sentence</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Sentence"/>
        <skos:definition>sentence that has been proven from other axioms and theorems</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Tool -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Tool">
        <skos:definition>software for processing DOL libraries and OMS.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Translation -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Translation">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Mapping"/>
        <skos:definition>mapping between languages or logics representing all structure, in contrast to reduction</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#translatableTo"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#Union -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#Union">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#DOLStructuredOMS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>aggregation of several OMS to a new OMS, without any renaming.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#UnsatisfiableOMS -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#UnsatisfiableOMS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#SatisfiableOMS"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMS"/>
        <skos:definition>OMS that is not satisfied by any model.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#View -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#View">
        <rdfs:label>view</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#OMSMapping"/>
        <skos:definition>OMS mapping that postulates a relation between two OMS</skos:definition>
        <rdfs:comment>a view (also called interpretation) between two heterogeneous OMS in a DOL library</rdfs:comment>
        <skos:note>An interpretation typically leads to proof obligations, \ie one has to prove that axioms of the source OMS of the mapping are theorems in the target OMS.</skos:note>
        <skos:note> When an interpretation is given as a set of correspondences, these are given as tuples, where the type of relationship is given by the specific kind of interpretation. </skos:note>
        <skos:altLabel>interpretation</skos:altLabel>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#WeakDefinitionalExtension -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#WeakDefinitionalExtension">
        <rdfs:label>weak definitional extension</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#Extension"/>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a weak definitional extension, if each model of $O_1$ can be expanded to at most one model of $O_2$.</skos:note>
        <skos:definition>extension whose newly introduced non-logical symbols can be interpreted in at most one way.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#WeakDefinitionalExtensionMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#WeakDefinitionalExtensionMapping">
        <rdfs:label>weak definitional extension mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#ExtensionMapping"/>
        <skos:note>An extension $O_2$ of an OMS $O_1$ is a weak definitional extension, if each model of $O_1$ can be expanded to at most one model of $O_2$.</skos:note>
        <skos:note>An extension is definitional if and only if it is both weakly definitional and model-theoretically conservative.</skos:note>
        <skos:definition>extension whose newly introduced non-logical symbols can be interpreted in at most one way</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#isWeakDefinitionalExtensionOf"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/DOL/Current/ontology#WeaklyExactMapping -->

    <owl:Class rdf:about="http://www.omg.org/spec/DOL/Current/ontology#WeaklyExactMapping">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#LogicMapping"/>
        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema-datatypes#boolean">true</propagatesToAdjoint>
        <skos:definition>logic mapping that is weakly compatible with certain DOL structuring constructs</skos:definition>
        <reifies rdf:resource="http://www.omg.org/spec/DOL/Current/ontology#weaklyExactlyMappableTo"/>
    </owl:Class>
    


