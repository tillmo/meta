#################################################################
#
#    Annotation properties
#
#################################################################


skos:prefLabel rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .



dol:propagatesToAdjoint rdf:type owl:AnnotationProperty ;
                        
                        rdfs:comment """Developers' note: This is just for informative purposes: Even though it probably does not go beyond OWL, it doesn't work for annotation properties in Protege.
Domain: owl:Class
Range: xsd:boolean"""@en ,
                                     "The subject class denotes a kind of mapping that, if it holds for a translation, also holds for the adjoint reduction."@en .



dol:reifies rdf:type owl:AnnotationProperty ;
            
            rdfs:comment """Developers' note: This is just for informative purposes: Even though it probably does not go beyond OWL, it doesn't work for annotation properties in Protege
Domain: owl:Class
Range: owl:ObjectProperty"""@en ,
                         "The subject class reifies the object property."@en .



dol:strictlyStrongerThan rdf:type owl:AnnotationProperty .



skos:editorialNote rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .



skos:example rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .



dol:reifiedBy rdf:type owl:AnnotationProperty ;
              
              rdfs:comment """Developers' note: This is just for informative purposes: It doesn't work for annotation properties
InverseOf: reifies"""@en ,
                           "The subject property is reified by the object class."@en .



skos:scopeNote rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .



skos:note rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .



dol:supportsSymbolKind rdf:type owl:AnnotationProperty ;
                       
                       rdfs:comment """Developers' note: This is just for informative purposes: Even though it probably does not go beyond OWL, it doesn't work for annotation properties in Protege
Domain: OMSLanguage
Range: owl:Class that rdfs:subClassOf value NonLogicalSymbol"""@en .



skos:definition rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .



dol:symbolKindIn rdf:type owl:AnnotationProperty ;
                 
                 rdfs:comment """Developers' note: This is just for informative purposes: It doesn't work for annotation properties
InverseOf: supportsSymbolKind"""@en .



skos:altLabel rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .





#################################################################
#
#    Datatypes
#
#################################################################




#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.omg.org/spec/DOL/DOL-terms/conservativelyExtendsTo

dol:conservativelyExtendsTo rdf:type owl:ObjectProperty ;
                            
                            rdfs:label "conservatively extends to"@en ;
                            
                            rdfs:comment "The object is a conservative extension of the subject"@en ;
                            
                            rdfs:domain dol:ConservativeExtensionMapping ;
                            
                            rdfs:subPropertyOf dol:extendsTo ,
                                               dol:isLogicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/definitionallyExtendsTo

dol:definitionallyExtendsTo rdf:type owl:ObjectProperty ;
                            
                            rdfs:label "definitionally extends to"@en ;
                            
                            rdfs:comment "the object is a definitional extension of the subject"@en ;
                            
                            rdfs:domain dol:DefinitionalExtensionMapping ;
                            
                            rdfs:subPropertyOf dol:conservativelyExtendsTo .



###  http://www.omg.org/spec/DOL/DOL-terms/entails

dol:entails rdf:type owl:ObjectProperty ;
            
            rdfs:label "entails"@en ;
            
            skos:definition "relation of logical entailment, expressing that all models of the subject are also models of the object"@en ;
            
            rdfs:subPropertyOf dol:isLogicallyRelatedTo ;
            
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( dol:OMS
                                        dol:OMSNetwork
                                      )
                        ] ;
            
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( dol:OMS
                                       dol:OMSNetwork
                                       dol:Sentence
                                     )
                       ] .



###  http://www.omg.org/spec/DOL/DOL-terms/extendsTo

dol:extendsTo rdf:type owl:ObjectProperty ;
              
              rdfs:label "extends to"@en ;
              
              rdfs:comment "The subject is a mapping of an extension, the object is the target of the extension mapping, i.e. the extended OMS."@en ;
              
              rdfs:domain dol:ExtensionMapping ;
              
              rdfs:range dol:OMS ;
              
              rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ,
                                 dol:mapsTo .



###  http://www.omg.org/spec/DOL/DOL-terms/formalizes

dol:formalizes rdf:type owl:ObjectProperty ;
               
               rdfs:label "formalizes"@en ;
               
               rdfs:comment "The subject is a precise mathematical entity capturing the object (which is an informal or semi-formal entity)"@en ;
               
               owl:inverseOf dol:isFormalizedBy ;
               
               rdfs:subPropertyOf dol:isSemanticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/hasAlignment

dol:hasAlignment rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "has alignment"@en ;
                 
                 rdfs:comment "The subject is a DOL library, in which the object is an alignment between two participating OMS."@en ;
                 
                 rdfs:range dol:Alignment ;
                 
                 rdfs:subPropertyOf dol:hasMapping ;
                 
                 owl:inverseOf dol:isAlignmentIn ;
                 
                 rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/hasExactLogicalExpressivity

dol:hasExactLogicalExpressivity rdf:type owl:ObjectProperty ;
                                
                                rdfs:label "has exact logical expressivity"@en ;
                                
                                rdfs:comment "The subject language has exactly the expressivity of the object logic."@en ;
                                
                                rdfs:subPropertyOf dol:isSemanticallyRelatedTo ,
                                                   dol:supportsLogic .



###  http://www.omg.org/spec/DOL/DOL-terms/hasHeterogeneousEnvironment

dol:hasHeterogeneousEnvironment rdf:type owl:ObjectProperty ;
                                
                                rdfs:label "has heterogeneous environment"@en ;
                                
                                skos:definition "relationship between the subject and a heterogeneous environment in which it is embedded or is associated with."@en ;
                                
                                rdfs:range dol:HeterogeneousEnvironment ;
                                
                                rdfs:subPropertyOf dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/hasInterpretation

dol:hasInterpretation rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "has interpretation"@en ;
                      
                      rdfs:comment "The subject is a DOL library, in which the object is an interpretation between two participating OMS."@en ;
                      
                      rdfs:range dol:Interpretation ;
                      
                      rdfs:subPropertyOf dol:hasMapping ;
                      
                      owl:inverseOf dol:isInterpretationIn ;
                      
                      rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/hasLanguageAspect

dol:hasLanguageAspect rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "has language aspect"@en ;
                      
                      skos:definition "the relation between an OMS language and its language aspects"@en ;
                      
                      rdfs:range dol:LanguageAspect ;
                      
                      rdfs:domain dol:OMSLanguage ;
                      
                      rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/hasLogic

dol:hasLogic rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:label "has logic"@en ;
             
             rdfs:comment """Developers' note: The reasoners in Protege don't like us to say the following, which makes hasLogic a non-simple property.  hasLogic is not used in cardinality restrictions, so it _should_ be OK to say the following, but in practice it doesn't work.
SubPropertyChain: semanticallyDenotes o hasLogic"""@en ;
             
             skos:definition "specification of valid reasoning that comprises signatures (user defined vocabularies), models (interpretations of these), sentences (constraints on models), and a satisfaction relation between models and sentences."@en ;
             
             rdfs:comment "the logic in which an OMS is formalized"@en ;
             
             rdfs:range dol:Logic ;
             
             rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
             
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( dol:OMS
                                         dol:OMSSemanticObject
                                       )
                         ] .



###  http://www.omg.org/spec/DOL/DOL-terms/hasMapping

dol:hasMapping rdf:type owl:ObjectProperty ;
               
               rdfs:label "has mapping"@en ;
               
               rdfs:comment "The subject is a DOL library, in which the object is a mapping between two participating OMS."@en ;
               
               rdfs:range dol:OMSMapping ;
               
               rdfs:subPropertyOf dol:hasMember ,
                                  dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/hasMember

dol:hasMember rdf:type owl:ObjectProperty ;
              
              rdfs:label "has member"@en ;
              
              skos:definition "The inverse of the set membership relation."@en ;
              
              rdfs:range dol:MathematicalEntity ;
              
              rdfs:domain dol:Set ;
              
              rdfs:subPropertyOf dol:isMathematicallyRelatedTo ;
              
              owl:inverseOf dol:isMemberOf .



###  http://www.omg.org/spec/DOL/DOL-terms/hasModels

dol:hasModels rdf:type owl:ObjectProperty ;
              
              rdfs:label "has models"@en ;
              
              skos:definition "an OMS (resp. its semantic object) has an assoicated class of models, which is the class of models of the OMS"@en ;
              
              rdfs:range dol:ClassOfModels ;
              
              rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
              
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( dol:OMS
                                          dol:OMSSemanticObject
                                        )
                          ] .



###  http://www.omg.org/spec/DOL/DOL-terms/hasOMS

dol:hasOMS rdf:type owl:ObjectProperty ;
           
           rdfs:label "has OMS"@en ;
           
           rdfs:comment "The subject is a DOL library, in which the object is a participating heterogeneous OMS."@en ;
           
           rdfs:range dol:OMS ;
           
           rdfs:subPropertyOf dol:hasMember ,
                              dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/hasPart

dol:hasPart rdf:type owl:ObjectProperty ;
            
            rdfs:label "has part"@en ;
            
            skos:definition "relationship between two things were the second is a component of the first"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/hasProficiency

dol:hasProficiency rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "has proficiency"@en ;
                   
                   skos:definition "property of a tool that is capable of implementing a process."@en ;
                   
                   rdfs:range dol:Process ;
                   
                   rdfs:domain dol:Tool .



###  http://www.omg.org/spec/DOL/DOL-terms/hasSentences

dol:hasSentences rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "has sentences"@en ;
                 
                 skos:definition "an OMS (resp. its semantic object) has an assoicated set of sentences, which is the set of sentences of the OMS"@en ;
                 
                 rdfs:range dol:SetOfSentences ;
                 
                 rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
                 
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( dol:OMS
                                             dol:OMSSemanticObject
                                           )
                             ] .



###  http://www.omg.org/spec/DOL/DOL-terms/hasSequenceMember

dol:hasSequenceMember rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "has sequence member"@en ;
                      
                      skos:definition "The inverse of the sequence membership relation."@en ;
                      
                      skos:note "this is a lingustic property, because it is about sequences of linguistic entities that occur in a text."@en ;
                      
                      rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ;
                      
                      owl:inverseOf dol:isMemberOf .



###  http://www.omg.org/spec/DOL/DOL-terms/hasSignature

dol:hasSignature rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "has signature"@en ;
                 
                 skos:definition "an OMS (resp. its semantic object) has an assoicated signature, which is the signature of the OMS"@en ;
                 
                 rdfs:range dol:Signature ;
                 
                 rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
                 
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( dol:OMS
                                             dol:OMSSemanticObject
                                           )
                             ] .



###  http://www.omg.org/spec/DOL/DOL-terms/hasSource

dol:hasSource rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;
              
              rdfs:label "has source"@en ;
              
              skos:definition "the source OMS of a mapping"@en ;
              
              rdfs:range dol:OMS ;
              
              rdfs:domain dol:OMSMapping ;
              
              rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/hasTarget

dol:hasTarget rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;
              
              rdfs:label "has target"@en ;
              
              skos:definition "the target OMS of a mapping"@en ;
              
              rdfs:range dol:OMS ;
              
              rdfs:domain dol:OMSMapping ;
              
              rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/impliedExtendsTo

dol:impliedExtendsTo rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "implied extends to"@en ;
                     
                     skos:definition "the relation of a model-theoretic conservative extension that does not introduce new non-logical symbols"@en ;
                     
                     rdfs:domain dol:ImpliedExtensionMapping ;
                     
                     rdfs:subPropertyOf dol:definitionallyExtendsTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isAdjointOf

dol:isAdjointOf rdf:type owl:ObjectProperty ;
                
                rdfs:label "is adjoint of"@en ;
                
                rdfs:comment "The subject is a translation (or reduction) that shares its sentence and model translation with the object reduction (or translation). The signature translations are adjoint to each other (in the sense of category theory)."@en ;
                
                rdfs:range dol:LogicReduction ;
                
                rdfs:domain dol:LogicTranslation ;
                
                rdfs:subPropertyOf dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isAlignmentIn

dol:isAlignmentIn rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "is alignment in"@en ;
                  
                  rdfs:comment "The obbject is a DOL library, in which the subject is an alignment between two participating OMS."@en ;
                  
                  rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isApproximantOf

dol:isApproximantOf rdf:type owl:ObjectProperty ,
                             owl:TransitiveProperty ;
                    
                    rdfs:label "is approximant of"@en ;
                    
                    skos:definition "approximation (in the sense of a logically implied theory, possibly aftersuitable translation) of an OMS in a smaller signature or OMS language"@en ;
                    
                    rdfs:domain dol:OMS ;
                    
                    rdfs:range dol:OMS ;
                    
                    rdfs:subPropertyOf dol:isEntailedBy .



###  http://www.omg.org/spec/DOL/DOL-terms/isCombinationOf

dol:isCombinationOf rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "is combination of OMS"@en ;
                    
                    skos:example "Consider an ontology involving a concept Person, and another one involving Human being, and an alignment that relates these to concepts. In the combination of the ontologies along the alignment, there is only one concept, representing both Person and Human being."@en ;
                    
                    skos:definition "aggregation of all the OMS in an OMS network, where non-logical symbols are shared according to the OMS mappings in the OMS network "@en ;
                    
                    rdfs:domain dol:Combination ;
                    
                    rdfs:range dol:OMSNetwork ;
                    
                    rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isCompositionOf

dol:isCompositionOf rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "is composition of"@en ;
                    
                    rdfs:comment "Developers' note: The subject mapping is the composition of the object list of mappings (list item 1 ∘ list item 2 ∘ … ∘ list item n).  All elements of the latter list must be instances of the same class as the subject.  (Enforcing the latter constraint goes beyond OWL.)"@en ,
                                 "Range can't be specified in OWL DL"@en ;
                    
                    rdfs:subPropertyOf dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isConsequenceTheoreticallyConservativeExtensionOf

dol:isConsequenceTheoreticallyConservativeExtensionOf rdf:type owl:ObjectProperty ;
                                                      
                                                      rdfs:label "is consequence theoretically conservative extension of"@en ;
                                                      
                                                      rdfs:comment "The subject OMS is a consequence-theoretic conservative extension of the object OMS. A consequence-theoretic conservative extension is an extension that does not add new theorems (in terms of the unextended signature)."@en ;
                                                      
                                                      rdfs:subPropertyOf dol:isConservativeExtensionOf .



###  http://www.omg.org/spec/DOL/DOL-terms/isConservativeExtensionOf

dol:isConservativeExtensionOf rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "is conservative extension of"@en ;
                              
                              rdfs:comment "the subject is a conservative extension of the object"@en ;
                              
                              rdfs:subPropertyOf dol:isExtensionOf ,
                                                 dol:isLogicallyRelatedTo ;
                              
                              owl:inverseOf dol:isModuleOf ;
                              
                              owl:propertyChainAxiom ( dol:isMappedBy
                                                       dol:conservativelyExtendsTo
                                                     ) .



###  http://www.omg.org/spec/DOL/DOL-terms/isDefinitionalExtensionOf

dol:isDefinitionalExtensionOf rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "is definitional extension of"@en ;
                              
                              skos:note " $O_2$ being a definitional extension of $O_1$ implies a bijective correspondence between the classes of models of $O_2$ and $O_1$. "@en ,
                                        " An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$."@en ,
                                        " Each definitional extension is also a monomorphic extension but not vice versa. "@en ;
                              
                              rdfs:comment "Developers' note: We need FOL, or the same reification approach as chosen for the different kinds of translations, for capturing that an extension is definitional if and only if it is weakly definitional and model-theoretically conservative."@en ;
                              
                              skos:definition "extension whose newly introduced non-logical symbols are interpreted in a unique way"@en ;
                              
                              rdfs:subPropertyOf dol:isModelTheoreticallyConservativeExtensionOf ;
                              
                              dol:strictlyStrongerThan dol:isMonomorphicExtensionOf ;
                              
                              rdfs:subPropertyOf dol:isMonomorphicExtensionOf ,
                                                 dol:isWeakDefinitionalExtensionOf ;
                              
                              owl:propertyChainAxiom ( dol:isMappedBy
                                                       dol:definitionallyExtendsTo
                                                     ) .



###  http://www.omg.org/spec/DOL/DOL-terms/isElementOf

dol:isElementOf rdf:type owl:ObjectProperty ;
                
                rdfs:label "is element of OMS"@en ;
                
                skos:definition "any resource in an OMS (e.g. a non-logical symbol, a sentence, a correspondence, the OMS itself, ...) or a named set of such resources."@en ;
                
                rdfs:range dol:OMS ;
                
                rdfs:subPropertyOf dol:hasPart ,
                                   dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isEmbeddableInto

dol:isEmbeddableInto rdf:type owl:ObjectProperty ,
                              owl:TransitiveProperty ;
                     
                     rdfs:label "is embeddable into"@en ;
                     
                     rdfs:comment "The subject is embeddable into the object logic."@en ;
                     
                     rdfs:subPropertyOf dol:isExactlyMappableTo ,
                                        dol:isMappableToBijectivelyOnModels ,
                                        dol:isMappableToLogic ,
                                        dol:isTranslatableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isEntailedBy

dol:isEntailedBy rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "is entailed by"@en ;
                 
                 skos:definition "inverse of relation of logical entailment"@en ;
                 
                 owl:inverseOf dol:entails ;
                 
                 rdfs:subPropertyOf dol:isLogicallyRelatedTo ;
                 
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( dol:OMS
                                             dol:OMSNetwork
                                             dol:Sentence
                                           )
                             ] ;
                 
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( dol:OMS
                                            dol:OMSNetwork
                                          )
                            ] .



###  http://www.omg.org/spec/DOL/DOL-terms/isExactlyMappableTo

dol:isExactlyMappableTo rdf:type owl:ObjectProperty ,
                                 owl:TransitiveProperty ;
                        
                        rdfs:label "is exactly mappable to"@en ;
                        
                        rdfs:comment "There is an exact mapping from the subject to the object logic."@en ;
                        
                        rdfs:subPropertyOf dol:isMathematicallyRelatedTo ,
                                           dol:isWeaklyExactlyMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isExtensionOf

dol:isExtensionOf rdf:type owl:ObjectProperty ,
                           owl:TransitiveProperty ;
                  
                  rdfs:comment "Relation between two OMS. The subject OMS is an extension of the object OMS, i.e. the sets of non-logical symbols and sentences of the subject OMS are supersets of those of the object OMS."@en ;
                  
                  skos:prefLabel "extends"@en ;
                  
                  skos:altLabel "is extension of"@en ;
                  
                  rdfs:domain dol:OMS ;
                  
                  rdfs:range dol:OMS ;
                  
                  rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ;
                  
                  owl:inverseOf dol:isSubOMSOf ;
                  
                  owl:propertyChainAxiom ( dol:isMappedBy
                                           dol:extendsTo
                                         ) .



###  http://www.omg.org/spec/DOL/DOL-terms/isFaithfullyMappableTo

dol:isFaithfullyMappableTo rdf:type owl:ObjectProperty ,
                                    owl:TransitiveProperty ;
                           
                           rdfs:label "is faithfully mappable to"@en ;
                           
                           rdfs:comment "There is a faithful mapping from the subject to the object logic."@en ;
                           
                           rdfs:subPropertyOf dol:isMappableToLogic ,
                                              dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isFormalizedBy

dol:isFormalizedBy rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "is formalized by"@en ;
                   
                   rdfs:comment "The object is a precise mathematical entity capturing the subject (which is an informal or semi-formal entity)"@en ;
                   
                   rdfs:subPropertyOf dol:isSemanticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isImpliedExtensionOf

dol:isImpliedExtensionOf rdf:type owl:ObjectProperty ;
                         
                         rdfs:label "is implied extension of"@en ;
                         
                         skos:note " A conservative extension $O_2$ of an OMS $O_1$ is an implied extension, if and only if the signature of $O_2$ is the signature of $O_1$. $O_2$ is an implied extension of $O_1$ if and only if the model class of $O_2$ is the model class of $O_1$."@en ,
                                   "Each implied extension is also a definitional extension but not vice versa."@en ;
                         
                         skos:definition "model-theoretic conservative extension that does not introduce new non-logical symbols"@en ;
                         
                         dol:strictlyStrongerThan dol:isConservativeExtensionOf ;
                         
                         rdfs:subPropertyOf dol:isDefinitionalExtensionOf ;
                         
                         owl:propertyChainAxiom ( dol:isMappedBy
                                                  dol:impliedExtendsTo
                                                ) .



###  http://www.omg.org/spec/DOL/DOL-terms/isInterpretationIn

dol:isInterpretationIn rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "is interpretation in"@en ;
                       
                       rdfs:comment "The object is a DOL library, in which the subject is an interpretation between two participating OMS."@en ;
                       
                       rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isLanguageAspectOf

dol:isLanguageAspectOf rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "is language aspect of"@en ;
                       
                       skos:definition "a language asecpt is a set of language constructs of a given language, not necessarily forming a sublanguage"@en ;
                       
                       owl:inverseOf dol:hasLanguageAspect ;
                       
                       rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isLanguageOf

dol:isLanguageOf rdf:type owl:FunctionalProperty ,
                          owl:ObjectProperty ;
                 
                 rdfs:label "is language of"@en ;
                 
                 rdfs:comment """Developers' note: The reasoners in Protege don't like us to say the following.  Together with the SubPropertyChain specified for supportsSerialization it would go beyond OWL DL anyway.
SubPropertyChain: serialization o serializes"""@en ;
                 
                 skos:definition "the language of an OMS"@en ;
                 
                 rdfs:domain dol:OMS ;
                 
                 rdfs:range dol:OMSLanguage ;
                 
                 rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isLinguisticallyRelatedTo

dol:isLinguisticallyRelatedTo rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "is linguistically related to"@en ;
                              
                              skos:definition "relation between linguistic entities."@en ;
                              
                              rdfs:domain dol:LinguisticEntity ;
                              
                              rdfs:range dol:LinguisticEntity .



###  http://www.omg.org/spec/DOL/DOL-terms/isLogicallyInteroperableWith

dol:isLogicallyInteroperableWith rdf:type owl:ObjectProperty ,
                                          owl:SymmetricProperty ;
                                 
                                 rdfs:label "is logically interoperable with"@en ;
                                 
                                 skos:definition "property of structured OMS, which may be written in different OMS languages supporting different logics, of being usable jointly in a coherent way (via suitable OMS language translations), such that the notions of their overall consistency and logical entailment have a precise logical semantics"@en ;
                                 
                                 rdfs:domain dol:StructuredOMS ;
                                 
                                 rdfs:range dol:StructuredOMS ;
                                 
                                 rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isLogicallyRelatedTo

dol:isLogicallyRelatedTo rdf:type owl:ObjectProperty ;
                         
                         rdfs:label "is logically related to"@en ;
                         
                         skos:definition "relation between lingustic entities that has a logical, semantic nature."@en ;
                         
                         rdfs:range dol:LinguisticEntity ;
                         
                         rdfs:domain dol:LinguisticEntity ;
                         
                         rdfs:subPropertyOf dol:isSemanticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isMappableFrom

dol:isMappableFrom rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "is mappable from"@en ;
                   
                   rdfs:comment "There is a mapping from the object to the subject."@en ;
                   
                   owl:inverseOf dol:isMappableTo ;
                   
                   rdfs:subPropertyOf dol:isRelatedAsPartOfMapsTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isMappableFromLanguage

dol:isMappableFromLanguage rdf:type owl:ObjectProperty ;
                           
                           rdfs:label "mappable from language"@en ;
                           
                           rdfs:comment "There is a mapping from the object language to the subject language."@en ;
                           
                           rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ,
                                              dol:isRelatedAsPartOfMapsTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isMappableFromLogic

dol:isMappableFromLogic rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "mappable from logic"@en ;
                        
                        rdfs:comment "There is a mapping from the object logic to the subject logic."@en ;
                        
                        rdfs:subPropertyOf dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isMappableTo

dol:isMappableTo rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 
                 rdfs:label "is mappable to"@en ;
                 
                 rdfs:comment "There is a mapping from the subject to the object."@en ;
                 
                 rdfs:subPropertyOf dol:isRelatedAsPartOfMapsTo ;
                 
                 owl:propertyChainAxiom ( dol:isSourceOfMapping
                                          dol:mapsTo
                                        ) .



###  http://www.omg.org/spec/DOL/DOL-terms/isMappableToBijectivelyOnModels

dol:isMappableToBijectivelyOnModels rdf:type owl:ObjectProperty ,
                                             owl:TransitiveProperty ;
                                    
                                    rdfs:label "is mappable to bijectively on models"@en ;
                                    
                                    rdfs:comment "There is a model-bijective mapping from the subject to the object logic."@en ;
                                    
                                    rdfs:subPropertyOf dol:isModelExpansivelyMappableTo ,
                                                       dol:isWeaklyExactlyMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isMappableToLanguage

dol:isMappableToLanguage rdf:type owl:ObjectProperty ,
                                  owl:TransitiveProperty ;
                         
                         rdfs:label "is mappable to language"@en ;
                         
                         rdfs:comment "There is a mapping from the subject language to the object language."@en ;
                         
                         rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ;
                         
                         owl:inverseOf dol:isMappableFromLanguage ;
                         
                         rdfs:subPropertyOf dol:isMappableTo ,
                                            dol:isRelatedAsPartOfMapsTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isMappableToLogic

dol:isMappableToLogic rdf:type owl:ObjectProperty ,
                               owl:TransitiveProperty ;
                      
                      rdfs:label "is mappable to logic"@en ;
                      
                      rdfs:comment "There is a mapping from the subject logic to the object logic."@en ;
                      
                      owl:inverseOf dol:isMappableFromLogic ;
                      
                      rdfs:subPropertyOf dol:isMappableTo ,
                                         dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isMappedBy

dol:isMappedBy rdf:type owl:ObjectProperty ;
               
               rdfs:label "is mapped by"@en ;
               
               rdfs:comment "The object mapping maps from the subject into something of the same type as the subject."@en ;
               
               rdfs:subPropertyOf dol:isRelatedAsPartOfMapsTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isMappingIn

dol:isMappingIn rdf:type owl:ObjectProperty ;
                
                rdfs:label "is mapping in"@en ;
                
                rdfs:comment "The object is a DOL library, in which the subject is a mapping between two participating OMS."@en ;
                
                owl:inverseOf dol:hasMapping ;
                
                rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isMathematicallyRelatedTo

dol:isMathematicallyRelatedTo rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "is mathematically related to"@en ;
                              
                              skos:definition "relation between mathematical entities."@en ;
                              
                              rdfs:range dol:MathematicalEntity ;
                              
                              rdfs:domain dol:MathematicalEntity .



###  http://www.omg.org/spec/DOL/DOL-terms/isMemberOf

dol:isMemberOf rdf:type owl:ObjectProperty ;
               
               rdfs:label "is member of"@en ;
               
               skos:definition "The set membership relation."@en ;
               
               rdfs:domain dol:MathematicalEntity ;
               
               rdfs:range dol:Set ;
               
               rdfs:subPropertyOf dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isModelExpansivelyMappableTo

dol:isModelExpansivelyMappableTo rdf:type owl:ObjectProperty ,
                                          owl:TransitiveProperty ;
                                 
                                 rdfs:label "is model expansively mappable to"@en ;
                                 
                                 rdfs:comment "There is a model-expansive mapping from the subject to the object logic."@en ;
                                 
                                 rdfs:subPropertyOf dol:isFaithfullyMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isModelOfOMS

dol:isModelOfOMS rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "is a model of the OMS"@en ;
                 
                 skos:definition "relationship between a model and an OMS where the model is a member of the model class of the OMS (which for flattenable OMS is equivalent to being a model of the signature of the OMS and satisfying all sentences in the OMS)."@en ;
                 
                 rdfs:domain dol:Model ;
                 
                 rdfs:range dol:OMS ;
                 
                 rdfs:subPropertyOf dol:isSemanticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isModelOfSignature

dol:isModelOfSignature rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "is a model of the signature"@en ;
                       
                       skos:definition "relationship between a model and a signature (vocabulary) such that the model provides a semantic interpretation of all non-logical symbols in the signature"@en ;
                       
                       rdfs:domain dol:Model ;
                       
                       rdfs:range dol:Signature ;
                       
                       rdfs:subPropertyOf dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isModelTheoreticallyConservativeExtensionOf

dol:isModelTheoreticallyConservativeExtensionOf rdf:type owl:ObjectProperty ;
                                                
                                                rdfs:label "is model theoretically conservative extension of"@en ;
                                                
                                                rdfs:comment "The subject OMS is a model-theoretic conservative extension of the object OMS. A model-theoretic conservative extension is an extension that does not lead to a restriction of class of models of an OMS."@en ;
                                                
                                                dol:strictlyStrongerThan dol:isConsequenceTheoreticallyConservativeExtensionOf ;
                                                
                                                rdfs:subPropertyOf dol:isConservativeExtensionOf .



###  http://www.omg.org/spec/DOL/DOL-terms/isModuleOf

dol:isModuleOf rdf:type owl:ObjectProperty ;
               
               rdfs:label "is module of"@en ;
               
               skos:definition "relationship between two OMS where the first conservatively extends to the second OMS"@en ;
               
               rdfs:subPropertyOf dol:isLogicallyRelatedTo ,
                                  dol:isSubOMSOf .



###  http://www.omg.org/spec/DOL/DOL-terms/isMonomorphicExtensionOf

dol:isMonomorphicExtensionOf rdf:type owl:ObjectProperty ;
                             
                             rdfs:label "is monomorphic extension of"@en ;
                             
                             rdfs:comment "The subject OMS is a monomorphic extension of the object OMS. A monomorphic extension is an extension whose newly introduced non-logical symbols are interpreted in a way unique up to isomorphism."@en ;
                             
                             rdfs:subPropertyOf dol:isModelTheoreticallyConservativeExtensionOf ;
                             
                             dol:strictlyStrongerThan dol:isModelTheoreticallyConservativeExtensionOf .



###  http://www.omg.org/spec/DOL/DOL-terms/isOMSDocumentationOf

dol:isOMSDocumentationOf rdf:type owl:FunctionalProperty ,
                                  owl:ObjectProperty ;
                         
                         rdfs:label "is OMS documentation of"@en ;
                         
                         skos:definition "relationship between an OMS documentation and the documented OMS"@en ;
                         
                         rdfs:range dol:OMS ;
                         
                         rdfs:domain dol:OMSDocumentation ;
                         
                         rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isOMSUnionOf

dol:isOMSUnionOf rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "is union of OMS"@en ;
                 
                 skos:definition "structured OMS expressing the combination of all constraints of a sequences of OMS"@en ;
                 
                 rdfs:domain dol:DOLStructuredOMS ;
                 
                 rdfs:range dol:SequenceOfOMS ;
                 
                 rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isOmsIn

dol:isOmsIn rdf:type owl:ObjectProperty ;
            
            rdfs:label "is OMS in"@en ;
            
            rdfs:comment "The subject is an OMS participating in a DOL library, which is the object."@en ;
            
            owl:inverseOf dol:hasOMS ;
            
            rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isPlainlyMappableTo

dol:isPlainlyMappableTo rdf:type owl:ObjectProperty ,
                                 owl:TransitiveProperty ;
                        
                        rdfs:label "is plainly mappable to"@en ;
                        
                        rdfs:comment "There is a plain mapping from the subject to the object logic."@en ;
                        
                        rdfs:subPropertyOf dol:isSimplyTheoroidallyMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isProfileOf

dol:isProfileOf rdf:type owl:ObjectProperty ;
                
                rdfs:label "is profile of"@en ;
                
                rdfs:comment "The subject is a isProfile of the object language.  This is a stronger notion than just being a sublanguage.  Supported serializations are shared along this property."@en ;
                
                skos:definition "see Class 'profile'"@en ;
                
                rdfs:range dol:OMSLanguage ;
                
                rdfs:domain dol:Profile ;
                
                rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ,
                                   dol:isSubLanguageOf .



###  http://www.omg.org/spec/DOL/DOL-terms/isReducibleFrom

dol:isReducibleFrom rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "is reducible from"@en ;
                    
                    rdfs:comment "There is a reduction from the object to the subject."@en ;
                    
                    rdfs:subPropertyOf dol:isMappableFrom .



###  http://www.omg.org/spec/DOL/DOL-terms/isReducibleTo

dol:isReducibleTo rdf:type owl:ObjectProperty ,
                           owl:TransitiveProperty ;
                  
                  rdfs:label "is reducible to"@en ;
                  
                  rdfs:comment "There is a reduction from the subject to the object."@en ;
                  
                  rdfs:subPropertyOf dol:isMappableTo ,
                                     dol:isMathematicallyRelatedTo ;
                  
                  owl:inverseOf dol:isReducibleFrom .



###  http://www.omg.org/spec/DOL/DOL-terms/isRelatedAsPartOfMapsTo

dol:isRelatedAsPartOfMapsTo rdf:type owl:ObjectProperty ;
                            
                            rdfs:label "is related as part of maps to"@en ;
                            
                            skos:definition "property used in the context of mappings, for both linguistic and mathematical entities."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/isSemanticallyRelatedTo

dol:isSemanticallyRelatedTo rdf:type owl:ObjectProperty ;
                            
                            rdfs:label "is semantically related to"@en ;
                            
                            skos:definition "relation that has a semantic nature."@en ;
                            
                            rdfs:domain [ rdf:type owl:Class ;
                                          owl:unionOf ( dol:LinguisticEntity
                                                        dol:MathematicalEntity
                                                        dol:SemanticEntity
                                                      )
                                        ] ;
                            
                            rdfs:range [ rdf:type owl:Class ;
                                         owl:unionOf ( dol:LinguisticEntity
                                                       dol:MathematicalEntity
                                                       dol:SemanticEntity
                                                     )
                                       ] .



###  http://www.omg.org/spec/DOL/DOL-terms/isSequenceMemberOf

dol:isSequenceMemberOf rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "is sequence memberOf"@en ;
                       
                       skos:definition "The sequence membership relation."@en ;
                       
                       owl:inverseOf dol:hasSequenceMember ;
                       
                       rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isSerializedIn

dol:isSerializedIn rdf:type owl:FunctionalProperty ,
                            owl:ObjectProperty ;
                   
                   rdfs:label "is serialized in"@en ;
                   
                   rdfs:comment "the textual syntax in which an OMS is written"@en ;
                   
                   rdfs:domain dol:OMS ;
                   
                   rdfs:range dol:Serialization ;
                   
                   rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isSimplyTheoroidallyMappableTo

dol:isSimplyTheoroidallyMappableTo rdf:type owl:ObjectProperty ,
                                            owl:TransitiveProperty ;
                                   
                                   rdfs:label "is simply theoroidally mappable to"@en ;
                                   
                                   rdfs:comment "There is a simple theoroidal mapping from the subject to the object logic."@en ;
                                   
                                   rdfs:subPropertyOf dol:isMappableToLogic ,
                                                      dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isSourceOfMapping

dol:isSourceOfMapping rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "is source of mapping"@en ;
                      
                      rdfs:comment "The object mapping maps from the subject into something of the same type as the subject."@en ;
                      
                      rdfs:subPropertyOf dol:isRelatedAsPartOfMapsTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isSubLanguageOf

dol:isSubLanguageOf rdf:type owl:ObjectProperty ,
                             owl:TransitiveProperty ;
                    
                    rdfs:label "is sublanguage of"@en ;
                    
                    rdfs:comment "The subject is a sublanguage of the object language.  Supported logics are shared along the inverse of this property."@en ;
                    
                    skos:definition "syntactically specified subset of a given language, consisting of a subset of its terminal and nonterminal symbols and grammar rules"@en ;
                    
                    rdfs:domain dol:Language ;
                    
                    rdfs:range dol:Language ;
                    
                    rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ,
                                       dol:isMappableToLanguage ,
                                       dol:isTranslatableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isSubLogicOf

dol:isSubLogicOf rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 
                 rdfs:label "is sublogic of"@en ;
                 
                 rdfs:comment "The subject is a sublogic of the object."@en ;
                 
                 skos:definition "a logic that is a syntactic restriction of another logic, inheriting its semantics"@en ;
                 
                 rdfs:domain dol:Logic ;
                 
                 rdfs:range dol:Logic ;
                 
                 rdfs:subPropertyOf dol:isEmbeddableInto ,
                                    dol:isMathematicallyRelatedTo ;
                 
                 owl:propertyChainAxiom ( [ owl:inverseOf dol:hasExactLogicalExpressivity
                                          ]
                                          dol:isSubLanguageOf
                                          dol:hasExactLogicalExpressivity
                                        ) .



###  http://www.omg.org/spec/DOL/DOL-terms/isSubOMSOf

dol:isSubOMSOf rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               
               rdfs:label "is subOMS of"@en ;
               
               skos:definition "OMS whose sets of non-logical symbols and sentences are subsets of those present in a given larger OMS"@en ;
               
               rdfs:domain dol:OMS ;
               
               rdfs:range dol:OMS ;
               
               rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isTranslatableFrom

dol:isTranslatableFrom rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "is translatable from"@en ;
                       
                       rdfs:comment "There is a translation from the object to the subject."@en ;
                       
                       rdfs:subPropertyOf dol:isMappableFrom ;
                       
                       owl:inverseOf dol:isTranslatableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isTranslatableTo

dol:isTranslatableTo rdf:type owl:ObjectProperty ,
                              owl:TransitiveProperty ;
                     
                     rdfs:label "is translatable to"@en ;
                     
                     rdfs:comment "There is a translation from the subject to the object."@en ;
                     
                     rdfs:subPropertyOf dol:isMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isWeakDefinitionalExtensionOf

dol:isWeakDefinitionalExtensionOf rdf:type owl:ObjectProperty ;
                                  
                                  rdfs:label "is weak definitional extension of"@en ;
                                  
                                  rdfs:comment """The subject OMS is a weak definitional extension of the object OMS. A weak definitional extension is an extension whose newly introduced
   non-logical symbols can be interpreted in at most one way.""" ;
                                  
                                  rdfs:subPropertyOf dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/isWeaklyExactlyMappableTo

dol:isWeaklyExactlyMappableTo rdf:type owl:ObjectProperty ,
                                       owl:TransitiveProperty ;
                              
                              rdfs:label "is weakly exactly mappable to"@en ;
                              
                              rdfs:comment "There is a weakly exact mapping from the subject to the object logic."@en ;
                              
                              rdfs:subPropertyOf dol:isMappableToLogic .



###  http://www.omg.org/spec/DOL/DOL-terms/mapsFrom

dol:mapsFrom rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:label "maps from"@en ;
             
             rdfs:comment "Developers' note: enforcing the constraint that a mapping must map into something of the same type goes beyond OWL."@en ,
                          "The subject mapping maps from the object into something of the same type as the object."@en ;
             
             skos:definition "relationship between a mapping and its domain"@en ;
             
             rdfs:domain dol:Mapping ;
             
             owl:inverseOf dol:isMappedBy ;
             
             rdfs:subPropertyOf dol:isRelatedAsPartOfMapsTo ;
             
             owl:inverseOf dol:isSourceOfMapping .



###  http://www.omg.org/spec/DOL/DOL-terms/mapsTo

dol:mapsTo rdf:type owl:FunctionalProperty ,
                    owl:ObjectProperty ;
           
           rdfs:label "maps to"@en ;
           
           rdfs:comment "The subject mapping maps from something into the object (which is of the same type).  Enforcing the latter constraint goes beyond OWL."@en ;
           
           skos:definition "relationship between a mapping and its codomain"@en ;
           
           rdfs:domain dol:Mapping ;
           
           rdfs:subPropertyOf dol:isRelatedAsPartOfMapsTo .



###  http://www.omg.org/spec/DOL/DOL-terms/satisfies

dol:satisfies rdf:type owl:ObjectProperty ;
              
              rdfs:label "satisfies"@en ;
              
              dct:abstract ""@en ;
              
              skos:definition "relationship between a model and a sentence indicating that the sentence holds true in the model"@en ;
              
              rdfs:domain dol:Model ;
              
              rdfs:seeAlso dol:SatisfactionRelation ;
              
              rdfs:range dol:Sentence ;
              
              rdfs:subPropertyOf dol:isMathematicallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/semanticallyDenotes

dol:semanticallyDenotes rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "semantically denotes"@en ;
                        
                        skos:definition "relation between a linguistic entity and its semantic denotation."@en ;
                        
                        rdfs:domain dol:LinguisticEntity ;
                        
                        rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
                        
                        rdfs:range [ rdf:type owl:Class ;
                                     owl:unionOf ( dol:MathematicalEntity
                                                   dol:SemanticEntity
                                                 )
                                   ] .



###  http://www.omg.org/spec/DOL/DOL-terms/serializes

dol:serializes rdf:type owl:ObjectProperty ;
               
               rdfs:label "serializes"@en ;
               
               rdfs:comment "The subject is a serialization for the object language. For example, Manchester syntax serializes OWL"@en ;
               
               rdfs:range dol:OMSLanguage ;
               
               rdfs:domain dol:Serialization ;
               
               rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ;
               
               owl:inverseOf dol:supportsSerialization .



###  http://www.omg.org/spec/DOL/DOL-terms/sharesWith

dol:sharesWith rdf:type owl:ObjectProperty ;
               
               rdfs:label "shares with"@en ;
               
               skos:note """Sharing is always relative to a given OMS network that
  relates different OMS. That is, two given OMS symbols
  can share with respect to one OMS network, and not share with respect to some
  other OMS network."""@en ;
               
               skos:definition "property of OMS symbols being mapped to the same symbol when computing a combination of an OMS network."@en ;
               
               rdfs:range dol:NonLogicalSymbol ;
               
               rdfs:domain dol:NonLogicalSymbol ;
               
               rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/specifiesSemanticsOf

dol:specifiesSemanticsOf rdf:type owl:ObjectProperty ;
                         
                         rdfs:label "specifies semantics of"@en ;
                         
                         rdfs:comment "The semantics of [a subset of] the object language can be specified in terms of the subject logic."@en ;
                         
                         rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
                         
                         owl:inverseOf dol:supportsLogic .



###  http://www.omg.org/spec/DOL/DOL-terms/supportsLogic

dol:supportsLogic rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "supports logic"@en ,
                             "supportsL lgic"@en ;
                  
                  rdfs:comment "The semantics of [a subset of] the subject language can be specified in terms of the object logic."@en ;
                  
                  skos:definition "logic represented by a satisfaction relation of the language."@en ;
                  
                  rdfs:range dol:Logic ;
                  
                  rdfs:domain dol:OMSLanguage ;
                  
                  rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
                  
                  owl:propertyChainAxiom ( dol:supportsLogic
                                           [ owl:inverseOf dol:isSubLogicOf
                                           ]
                                         ) ,
                                         ( [ owl:inverseOf dol:isSubLanguageOf
                                           ]
                                           dol:supportsLogic
                                         ) .



###  http://www.omg.org/spec/DOL/DOL-terms/supportsLogicMapping

dol:supportsLogicMapping rdf:type owl:ObjectProperty ;
                         
                         rdfs:label "supports logic mapping"@en ;
                         
                         rdfs:comment "The subject language mapping corresponds to the object logic mapping."@en ;
                         
                         rdfs:range dol:LogicMapping ;
                         
                         rdfs:domain dol:OMSLanguageTranslation ;
                         
                         rdfs:subPropertyOf dol:isSemanticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/supportsSerialization

dol:supportsSerialization rdf:type owl:InverseFunctionalProperty ,
                                   owl:ObjectProperty ;
                          
                          rdfs:label "supports serialization"@en ;
                          
                          rdfs:comment """Developers' note: The reasoners in Protege don't like us to say the following.  Together with the SubPropertyChain specified for language it would go beyond OWL DL anyway.
SubPropertyChain: isProfileOf o supportsSerialization"""@en ,
                                       "Note that the serialization should be as specific as possible, i.e. one should not say that ``OWL can be serialized in XML'' and ``Common Logic can be serialized in XML'', but instead ``OWL can be serialized in OWL XML'' and ``Common Logic can be serialized in XCL'', taking into account that OWL XML and XCL are two different XML languages."@en ,
                                       "OMS in the subject language can be serialized in the object serialization."@en ;
                          
                          rdfs:domain dol:OMSLanguage ;
                          
                          rdfs:range dol:Serialization ;
                          
                          rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .



###  http://www.omg.org/spec/DOL/DOL-terms/usesLanguage

dol:usesLanguage rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "uses language"@en ;
                 
                 skos:definition "relation between something and a language used by one or more components of it."@en ;
                 
                 rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .





#################################################################
#
#    Data properties
#
#################################################################


###  http://www.omg.org/spec/DOL/DOL-terms/altFilenameExtension

dol:altFilenameExtension rdf:type owl:DatatypeProperty ;
                         
                         skos:definition "an alternative filename extension for a serialization"@en ;
                         
                         rdfs:domain dol:Serialization ;
                         
                         rdfs:range xsd:string .



###  http://www.omg.org/spec/DOL/DOL-terms/mediaType

dol:mediaType rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              
              rdfs:comment "the media type (MIME type) of a serialization"@en ;
              
              rdfs:domain dol:Serialization ;
              
              rdfs:range xsd:string .



###  http://www.omg.org/spec/DOL/DOL-terms/prefFilenameExtension

dol:prefFilenameExtension rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          
                          skos:definition "the preferred filename extension for a serialization"@en ;
                          
                          rdfs:domain dol:Serialization ;
                          
                          rdfs:range xsd:string .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.omg.org/spec/DOL/DOL-terms/AbstractSyntax

dol:AbstractSyntax rdf:type owl:Class ;
                   
                   rdfs:label "abstract syntax"@en ;
                   
                   rdfs:subClassOf dol:LinguisticEntity ;
                   
                   skos:note "An abstract syntax can be specified as a MOF metamodel. Then abstract abstract syntax documents can be represented as  XMI documents."@en ;
                   
                   skos:definition "term language for representing documents in a machine-processable way"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Alignment

dol:Alignment rdf:type owl:Class ;
              
              rdfs:label "alignment"@en ;
              
              rdfs:subClassOf dol:OMSMapping ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty dol:hasPart ;
                                owl:someValuesFrom dol:Correspondence
                              ] ;
              
              owl:disjointWith dol:Interpretation ;
              
              skos:definition "an OMS mapping expressing a collection of relations between entities of the two OMS"@en ;
              
              rdfs:comment "an alignment between two heterogeneous OMS in a DOL library"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/AnnotationExpression

dol:AnnotationExpression rdf:type owl:Class ;
                         
                         rdfs:label "annotation expression"@en ;
                         
                         rdfs:subClassOf dol:Expression ;
                         
                         skos:note " According to note~\\ref{note:annotation-rdf} it is possible to interpret annotations under an RDF semantics. ``Without a logical semantics'' in this definition means that annotations to an OMS are not considered sentences of that OMS. "@en ,
                                   "\\label{note:annotation-rdf} Formally, an annotation is given as a $(\\text{subject}, \\text{predicate}, \\text{object})$ triple as defined by \\nisref{SOURCE: W3C/TR REC-rdf-concepts:2004, Section 6}. The subject of an annotation is an element of an OMS. The predicate is an RDF property defined in an external OMS and describes in what way the annotation object is related to the annotation subject. "@en ;
                         
                         skos:definition "additional information without a logical semantics that is attached to an element of an OMS"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/AnnotationExpressionLanguageAspect

dol:AnnotationExpressionLanguageAspect rdf:type owl:Class ;
                                       
                                       rdfs:label "annotation expression language aspect"@en ;
                                       
                                       rdfs:subClassOf dol:LanguageAspect ;
                                       
                                       skos:definition "the (unique) language aspect of an OMS language that enables the expression of comments and annotations"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/AnswerSubstitution

dol:AnswerSubstitution rdf:type owl:Class ;
                       
                       rdfs:label "answer substitution"@en ;
                       
                       rdfs:subClassOf dol:Substitution ;
                       
                       skos:definition "substitution that, when applied to a given query, turns the latter into a logical consequence of a given OMS"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Approximant

dol:Approximant rdf:type owl:Class ;
                
                rdfs:label "approximant"@en ;
                
                rdfs:subClassOf dol:OMS ;
                
                skos:definition "approximation (in the sense of a logically implied theory, possibly after suitable translation)  of an OMS in a smaller signature or a sublanguage."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Approximation

dol:Approximation rdf:type owl:Class ;
                  
                  rdfs:label "approximation"@en ;
                  
                  rdfs:subClassOf dol:DOLStructuredOMS ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty dol:hasPart ;
                                    owl:someValuesFrom dol:DOLStructuredOMS
                                  ] ;
                  
                  skos:definition "structured OMS that expresses a maximum approximant."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/AxiomSentence

dol:AxiomSentence rdf:type owl:Class ;
                  
                  rdfs:label "axiom sentence"@en ;
                  
                  rdfs:subClassOf dol:Sentence ;
                  
                  skos:definition "sentence postulated to be valid (i.e.\\ true in every model)"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/BasicOMS

dol:BasicOMS rdf:type owl:Class ;
             
             rdfs:label "basic OMS"@en ;
             
             rdfs:subClassOf dol:FlattenableOMS ,
                             dol:NativeOMS ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty dol:hasPart ;
                               owl:onClass dol:OMSDocumentation ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty dol:semanticallyDenotes ;
                               owl:someValuesFrom dol:LogicalTheory
                             ] ;
             
             rdfs:comment "a basic OMS (in a single language, logic, and serialization)"@en ;
             
             skos:definition "unstructured native OMS that may be used as a building block for a larger OMS, denoting a logical theory"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Category

dol:Category rdf:type owl:Class ;
             
             rdfs:label "category"@en ;
             
             rdfs:subClassOf dol:MathematicalEntity ;
             
             skos:note "In DOL, objects of a category are usually signatures or OMS, and morphisms are signature morphisms, or OMS mappings. In principle, no assumption about the exact nature of objects and morphisms is made."@en ,
                       "The morphisms determine which part of the structure of the objects is relevant, i.e. preserved by morphisms. Hence, objects can be seen as 'sets with structure', and morphisms as 'structure-preserving maps'.  However note that not all categories can be obtained in this way."@en ;
             
             skos:definition "a class of objects with suitable morphisms between them"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ClassOfModels

dol:ClassOfModels rdf:type owl:Class ;
                  
                  rdfs:label "class of models"@en ;
                  
                  rdfs:subClassOf dol:SemanticEntity ;
                  
                  skos:definition "a class of models (over some signature in some logic)"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Closure

dol:Closure rdf:type owl:Class ;
            
            rdfs:label "closure"@en ;
            
            rdfs:subClassOf dol:DOLStructuredOMS ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty dol:hasPart ;
                              owl:someValuesFrom dol:DOLStructuredOMS
                            ] ;
            
            skos:note "DOL supports four different forms of closure: minimization and maximization as well as freeness and cofreeness."@en ,
                      "Symbols from the local environment are assumed to have a fixed interpretation. Only the symbols newly declared in the closure are forced to have minimal or maximal interpretation."@en ;
            
            skos:definition "structured OMS expressing a variant of the closed world assumption by restricting the models to those that are minimal or maximal (with respect to the local environment). The closed world assumption is a default assumption about facts whose status in unknown."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Cofreeness

dol:Cofreeness rdf:type owl:Class ;
               
               rdfs:label "cofreeness"@en ;
               
               rdfs:subClassOf dol:Closure ;
               
               skos:note "In first-order-like logics, cofreeness means maximal interpretation of predicates and equality being observable equivalence. Cofreeness can be used for the specification of coinductive datatypes like infinite lists and streams."@en ;
               
               skos:definition "special type of closure, restriction of models to those that are cofree (with respect to the local environment)."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/CollectionOfExpressions

dol:CollectionOfExpressions rdf:type owl:Class ;
                            
                            rdfs:label "collection of expressions"@en ;
                            
                            rdfs:subClassOf dol:LinguisticEntity ;
                            
                            skos:definition "collection whose members are expressions"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Combination

dol:Combination rdf:type owl:Class ;
                
                rdfs:label "combination"@en ;
                
                rdfs:subClassOf dol:DOLStructuredOMS ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:hasPart ;
                                  owl:someValuesFrom dol:OMSNetwork
                                ] ;
                
                skos:example "Consider an ontology involving a concept Person, and another one involving Human being, and an alignment that relates these to concepts. In the combination of the ontologies along the alignment, there is only one concept, representing both Person and Human being."@en ;
                
                skos:definition "structured OMS expressing the aggregation of all the OMS in an OMS network, where non-logical symbols are shared according to the OMS mappings in the OMS network "@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ConsequenceTheoreticConservativeExtension

dol:ConsequenceTheoreticConservativeExtension rdf:type owl:Class ;
                                              
                                              rdfs:label "consequence-theoretic conservative extension"@en ;
                                              
                                              rdfs:subClassOf dol:ConservativeExtension ;
                                              
                                              skos:note "An extension $O_2$ of an OMS $O_1$ is a consequence-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$."@en ;
                                              
                                              skos:definition "extension that does not add new theorems (in terms of the unextended signature)."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ConsequenceTheoreticConservativeExtensionMapping

dol:ConsequenceTheoreticConservativeExtensionMapping rdf:type owl:Class ;
                                                     
                                                     rdfs:label "consequence-theoretic conservative extension mapping"@en ;
                                                     
                                                     rdfs:subClassOf dol:ConservativeExtensionMapping ;
                                                     
                                                     skos:note "An extension $O_2$ of an OMS $O_1$ is a consequence-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$."@en ;
                                                     
                                                     skos:definition "extension that does not add new theorems (in terms of the unextended signature)"@en ;
                                                     
                                                     dol:reifies dol:isConsequenceTheoreticallyConservativeExtensionOf .



###  http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtension

dol:ConservativeExtension rdf:type owl:Class ;
                          
                          rdfs:label "conservative extension"@en ;
                          
                          rdfs:subClassOf dol:Extension ;
                          
                          skos:definition "extension that does not add new logical properties with respect to the signature of the extended OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtensionMapping

dol:ConservativeExtensionMapping rdf:type owl:Class ;
                                 
                                 rdfs:label "conservative extension mapping"@en ;
                                 
                                 rdfs:subClassOf dol:ExtensionMapping ;
                                 
                                 skos:note "If used without qualification, the consequence-theoretic version is meant."@en ;
                                 
                                 skos:definition "extension mapping of a consequence-theoretic or model-theoretic conservative extension"@en ;
                                 
                                 dol:reifies dol:isConservativeExtensionOf .



###  http://www.omg.org/spec/DOL/DOL-terms/ConsistentOMS

dol:ConsistentOMS rdf:type owl:Class ;
                  
                  rdfs:label "consistent OMS"@en ;
                  
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:intersectionOf ( dol:OMS
                                                             [ rdf:type owl:Class ;
                                                               owl:complementOf dol:InconsistentOMS
                                                             ]
                                                           )
                                      ] ;
                  
                  rdfs:subClassOf dol:OMS ;
                  
                  skos:definition "OMS that has a non-trivial set of logical consequences in the sense that not every sentence follows from the OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Correspondence

dol:Correspondence rdf:type owl:Class ;
                   
                   rdfs:label "correspondence"@en ;
                   
                   rdfs:subClassOf dol:DOLEntity ;
                   
                   skos:note """A correspondence is given as a quadruple 
$(e_1, R, e_2, c)$,
where $R$ denotes the type of relationship that is asserted to hold between the two non-logical 
symbols/terms, and 0<=c<=1 is a confidence value.  $R$ and $c$ may be omitted: When $R$ is 
omitted, it defaults to the equivalence relation, unless another default
relation has been explicitly specified; when $c$ is omitted, it defaults to 1."""@en ;
                   
                   skos:definition "relationship between an non-logical symbol $e_1$ from an OMS $O_1$ and an non-logical symbol $e_2$ from an OMS $O_2$, or between an non-logical symbol $e_1$ from $O_1$ and a term $t_2$ formed from non-logical symbols\\index{non-logical symbol from $O_2$."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/DOLDocument

dol:DOLDocument rdf:type owl:Class ;
                
                rdfs:label "DOL document"@en ;
                
                rdfs:subClassOf dol:DOLEntity ,
                                dol:Document ;
                
                skos:definition "document containing a DOL library."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/DOLEntity

dol:DOLEntity rdf:type owl:Class ;
              
              rdfs:label "DOL entity"@en ;
              
              rdfs:subClassOf dol:LinguisticEntity ;
              
              skos:definition "entity that is used in DOL texts."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/DOLLibrary

dol:DOLLibrary rdf:type owl:Class ;
               
               rdfs:label "DOL library"@en ;
               
               rdfs:subClassOf dol:DOLEntity ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty dol:semanticallyDenotes ;
                                 owl:someValuesFrom dol:GlobalEnvironment
                               ] ;
               
               skos:note "collection is used here in the sense of syntactic collection"@en ;
               
               skos:definition "collection of named OMS and OMS networks, possibly written in different OMS languages, linked by named OMS mappings."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS

dol:DOLStructuredOMS rdf:type owl:Class ;
                     
                     rdfs:label "DOL structured OMS"@en ;
                     
                     rdfs:subClassOf dol:StructuredOMS ;
                     
                     skos:note "DOL structured OMS, typically, use basic OMS as building blocks for defining other structured OMS, OMS mappings or OMS networks."@en ;
                     
                     skos:definition "syntactically valid DOL expression denoting an OMS that is built from smaller OMS as building blocks."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/DefaultMapping

dol:DefaultMapping rdf:type owl:Class ;
                   
                   rdfs:label "default mapping"@en ;
                   
                   rdfs:subClassOf dol:LogicMapping ;
                   
                   dol:propagatesToAdjoint "true"^^xsd:boolean ;
                   
                   skos:definition "logic mapping that will be chosen by default if only the source and target logic are given"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/DefinitionalExtension

dol:DefinitionalExtension rdf:type owl:Class ;
                          
                          rdfs:label "definitional extension"@en ;
                          
                          owl:equivalentClass [ rdf:type owl:Class ;
                                                owl:intersectionOf ( dol:ModelTheoreticConservativeExtension
                                                                     dol:WeakDefinitionalExtension
                                                                   )
                                              ] ;
                          
                          rdfs:subClassOf dol:MonomorphicExtension ;
                          
                          skos:note "An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$."@en ;
                          
                          skos:definition "extension whose newly introduced non-logical symbols are interpreted in a unique way."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/DefinitionalExtensionMapping

dol:DefinitionalExtensionMapping rdf:type owl:Class ;
                                 
                                 rdfs:label "definitional extension mapping"@en ;
                                 
                                 owl:equivalentClass [ rdf:type owl:Class ;
                                                       owl:intersectionOf ( dol:ModelTheoreticConservativeExtensionMapping
                                                                            dol:WeakDefinitionalExtensionMapping
                                                                          )
                                                     ] ;
                                 
                                 rdfs:subClassOf dol:MonomorphicExtensionMapping ;
                                 
                                 skos:note "$O_2$ being a definitional extension of $O_1$ implies a bijective correspondence between the classes of models of $O_2$ and $O_1$."@en ,
                                           "An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$."@en ,
                                           "Each definitional extension is also a monomorphic extension but not vice versa."@en ;
                                 
                                 skos:definition "extension whose newly introduced non-logical symbols are interpreted in a unique way"@en ;
                                 
                                 dol:reifies dol:isDefinitionalExtensionOf .



###  http://www.omg.org/spec/DOL/DOL-terms/Document

dol:Document rdf:type owl:Class ;
             
             rdfs:label "document"@en ;
             
             rdfs:subClassOf dol:LinguisticEntity ;
             
             skos:definition "result of serializing an OMS or DOL library using a given serialization."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ElusiveOMS

dol:ElusiveOMS rdf:type owl:Class ;
               
               rdfs:label "elusive OMS"@en ;
               
               rdfs:subClassOf dol:OMS ;
               
               owl:disjointWith dol:FlattenableOMS ;
               
               skos:definition "OMS that is not flattenable"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Embedding

dol:Embedding rdf:type owl:Class ;
              
              rdfs:label "embedding"@en ;
              
              rdfs:subClassOf dol:ExactMapping ,
                              dol:LogicMapping ,
                              dol:LogicTranslation ,
                              dol:ModelBijectiveMapping ;
              
              skos:definition "logic mapping that embeds the source into the target logic, using components that are embeddings and (in the case of model translations) isomorphism"@en ;
              
              dol:reifies dol:isEmbeddableInto .



###  http://www.omg.org/spec/DOL/DOL-terms/Entailment

dol:Entailment rdf:type owl:Class ;
               
               rdfs:label "entailment"@en ;
               
               rdfs:subClassOf dol:OMSMapping ;
               
               skos:note "Entailment expresses that each model satisfying the first OMS also satisfies the second OMS (or the sentence, respectively)."@en ,
                         "Entailments can also be stated between networks, or networks and OMS."@en ;
               
               skos:altLabel "logical consequence"@en ;
               
               skos:definition "relation between two OMS (or an OMS and a sentence) expressing that the second one is logically implied by the first one."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Equivalence

dol:Equivalence rdf:type owl:Class ;
                
                rdfs:label "equivalence"@en ;
                
                rdfs:subClassOf dol:OMSMapping ;
                
                skos:note " Two OMS are equivalent if they have a common definitional extension. The OMS may be written in different OMS languages. "@en ;
                
                skos:definition "OMS mapping ensuring that two OMS share the same definable concepts"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ExactMapping

dol:ExactMapping rdf:type owl:Class ;
                 
                 rdfs:label "exact mapping"@en ;
                 
                 rdfs:subClassOf dol:WeaklyExactMapping ;
                 
                 dol:propagatesToAdjoint "true"^^xsd:boolean ;
                 
                 skos:definition "logic mapping that is compatible with certain DOL structuring constructs"@en ;
                 
                 dol:reifies dol:isExactlyMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/Expression

dol:Expression rdf:type owl:Class ;
               
               rdfs:label "expression"@en ;
               
               rdfs:subClassOf dol:LinguisticEntity ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty dol:usesLanguage ;
                                 owl:onClass dol:Language ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                               ] ;
               
               skos:definition "a finite combination of symbols that are well-formed according to applicable rules (depending on the language)"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Extension

dol:Extension rdf:type owl:Class ;
              
              rdfs:label "extension"@en ;
              
              rdfs:subClassOf dol:DOLStructuredOMS ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty dol:hasPart ;
                                owl:someValuesFrom dol:DOLStructuredOMS
                              ] ;
              
              skos:note """The new symbols and sentences are interpreted relative to the
local envorinment, which is the signature of the 'given OMS'."""@en ;
              
              skos:definition "structured OMS extending a given OMS with new symbols and sentences."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ExtensionMapping

dol:ExtensionMapping rdf:type owl:Class ;
                     
                     rdfs:label "extension mapping"@en ;
                     
                     rdfs:subClassOf dol:OMSMapping ;
                     
                     skos:editorialNote "This definition differs from that in the Terms and Definition section of the DOL specification by focussing on the mapping rather than the codomain of the mapping."@en ;
                     
                     skos:definition "mapping between two OMS where the sets of non-logical symbols and sentences of the second OMS are supersets of those present in the first OMS"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/FaithfulMapping

dol:FaithfulMapping rdf:type owl:Class ;
                    
                    rdfs:label "faithful mapping"@en ;
                    
                    rdfs:subClassOf dol:LogicMapping ;
                    
                    dol:propagatesToAdjoint "true"^^xsd:boolean ;
                    
                    skos:definition "logic mapping that preserves and reflects logical consequence"@en ;
                    
                    dol:reifies dol:isFaithfullyMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/Filtering

dol:Filtering rdf:type owl:Class ;
              
              rdfs:label "filtering"@en ;
              
              rdfs:subClassOf dol:DOLStructuredOMS ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty dol:hasPart ;
                                owl:someValuesFrom dol:DOLStructuredOMS
                              ] ;
              
              skos:note "If a symbol is removed, all axioms containing that symbol are removed, too."@en ;
              
              skos:definition "structured OMS expressing the verbatim removal of symbols or axioms from an OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/FlattenableOMS

dol:FlattenableOMS rdf:type owl:Class ;
                   
                   rdfs:label "flattenable OMS"@en ;
                   
                   rdfs:subClassOf dol:OMS ;
                   
                   skos:note "More precisely, an OMS is flattenable if and only if it is either a basic OMS or it is an extension, union, translation, module extraction, approximation, filtering, or reference of named OMS involving only flattenable OMS."@en ;
                   
                   skos:definition "OMS that can be seen, by purely syntactical means, to be logically equivalent to a basic OMS"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Flattening

dol:Flattening rdf:type owl:Class ;
               
               rdfs:label "flattening"@en ;
               
               rdfs:subClassOf dol:Process ;
               
               skos:definition "process of transforming an OMS to an equivalent basic OMS"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/FormalSemantics

dol:FormalSemantics rdf:type owl:Class ;
                    
                    rdfs:label "formal semantics"@en ;
                    
                    rdfs:subClassOf dol:SemanticEntity ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty dol:hasPart ;
                                      owl:someValuesFrom dol:SemanticRule
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty dol:formalizes ;
                                      owl:someValuesFrom dol:Language
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty dol:hasPart ;
                                      owl:someValuesFrom dol:SemanticDomain
                                    ] ;
                    
                    skos:note "A formal semantics is a formalization of the meaning of a language."@en ;
                    
                    skos:definition "assignment of a mathematical meaning to a language by mapping abstract syntax to suitable semantic domains"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Freeness

dol:Freeness rdf:type owl:Class ;
             
             rdfs:label "freeness"@en ;
             
             rdfs:subClassOf dol:Closure ;
             
             skos:note "In first-order logic (and similar logics), freeness means minimal interpretation of predicates and minimal equality among data values. Freeness can be used for the specification of inductive datatypes like numbers, lists, trees, bags etc. In order to specify e.g. lists over some elements, the specification of the elements should be in the local environment."@en ;
             
             skos:definition "special type of closure, restriction of models to those that are free (with respect to the local environment)."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/GlobalEnvironment

dol:GlobalEnvironment rdf:type owl:Class ;
                      
                      rdfs:label "global environment"@en ;
                      
                      rdfs:subClassOf dol:MathematicalEntity ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty dol:hasPart ;
                                        owl:someValuesFrom dol:LocalEnvironment
                                      ] ;
                      
                      skos:definition "mapping from identifiers (IRIs) to values in  semantics domains representing the global knowledge about OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Graph

dol:Graph rdf:type owl:Class ;
          
          rdfs:label "graph"@en ;
          
          rdfs:subClassOf dol:MathematicalEntity ;
          
          skos:altLabel "network"@en ;
          
          skos:definition "set of objects (nodes) that are connected by links (edges)"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/HeterogeneousEnvironment

dol:HeterogeneousEnvironment rdf:type owl:Class ;
                             
                             rdfs:label "heterogeneous environment"@en ;
                             
                             rdfs:subClassOf dol:MathematicalEntity ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty dol:hasPart ;
                                               owl:someValuesFrom dol:SupportsRelation
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty dol:hasPart ;
                                               owl:someValuesFrom dol:LanguageGraph
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty dol:hasPart ;
                                               owl:someValuesFrom dol:LogicGraph
                                             ] ;
                             
                             skos:definition "environment for the expression of homogeneous and heterogeneous OMS, comprising a logic graph, an OMS language graph and a supports relation"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/HeterogeneousOMS

dol:HeterogeneousOMS rdf:type owl:Class ;
                     
                     rdfs:label "heterogeneous OMS"@en ;
                     
                     rdfs:subClassOf dol:OMS ;
                     
                     owl:disjointWith dol:HomogeneousOMS ;
                     
                     skos:definition "OMS whose parts are supported by different logics"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/HomogeneousOMS

dol:HomogeneousOMS rdf:type owl:Class ;
                   
                   rdfs:label "homogeneous OMS"@en ;
                   
                   rdfs:subClassOf dol:OMS ;
                   
                   skos:definition "OMS whose parts are supported by one logic"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ImpliedExtension

dol:ImpliedExtension rdf:type owl:Class ;
                     
                     rdfs:label "implied extension"@en ;
                     
                     rdfs:subClassOf dol:DefinitionalExtension ;
                     
                     skos:note """A conservative extension $O_2$ of an OMS
    $O_1$ is an implied extension, if and only if the signature of
    $O_2$ is the signature of $O_1$.  $O_2$ is an implied extension of
    $O_1$ if and only if the model class of $O_2$ is the model class
    of $O_1$."""@en ;
                     
                     skos:definition "model-theoretic conservative extension that does not introduce new non-logical symbols."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ImpliedExtensionMapping

dol:ImpliedExtensionMapping rdf:type owl:Class ;
                            
                            rdfs:label "implied extension mapping"@en ;
                            
                            rdfs:subClassOf dol:DefinitionalExtensionMapping ;
                            
                            skos:note "A conservative extension $O_2$ of an OMS $O_1$ is an implied extension, if and only if the signature of $O_2$ is the signature of $O_1$. $O_2$ is an implied extension of $O_1$ if and only if the model class of $O_2$ is the model class of $O_1$."@en ,
                                      "Each implied extension is also a definitional extension but not vice versa."@en ;
                            
                            skos:definition "model-theoretic conservative extension that does not introduce new non-logical symbols"@en ;
                            
                            dol:reifies dol:isImpliedExtensionOf .



###  http://www.omg.org/spec/DOL/DOL-terms/Import

dol:Import rdf:type owl:Class ;
           
           rdfs:label "import"@en ;
           
           rdfs:subClassOf dol:DOLStructuredOMS ;
           
           skos:example "An owl:import in OWL is an import."@en ;
           
           skos:note "Importing $O_1$ into $O_2$ turns $O_2$ into an extension of $O_1$."@en ,
                     "Semantically, an import of $O_2$ into $O_1$ is equivalent to the verbatim inclusion of $O_2$ in place of the import declaration."@en ,
                     "The import of a whole DOL library into another DOL library is also called import."@en ,
                     "The purpose of $O_2$ importing $O_1$ is to make non-logical symbols and sentences of $O_1$ available in $O_2$."@en ;
           
           skos:definition "reference to an OMS behaving as if it were verbatim included; also import of DOL libraries."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/InconsistentOMS

dol:InconsistentOMS rdf:type owl:Class ;
                    
                    rdfs:label "inconsistent OMS"@en ;
                    
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:intersectionOf ( dol:OMS
                                                               [ rdf:type owl:Class ;
                                                                 owl:complementOf dol:ConsistentOMS
                                                               ]
                                                             )
                                        ] ;
                    
                    rdfs:subClassOf dol:UnsatisfiableOMS ;
                    
                    skos:definition "OMS that has a trivial set of logical consequences in the sense that every sentence follows from the OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Institution

dol:Institution rdf:type owl:Class ;
                
                rdfs:label "institution"@en ;
                
                rdfs:subClassOf dol:MathematicalEntity ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:hasPart ;
                                  owl:someValuesFrom dol:Category
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:hasPart ;
                                  owl:someValuesFrom dol:SignatureMorphism
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:formalizes ;
                                  owl:someValuesFrom dol:Logic
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:hasPart ;
                                  owl:someValuesFrom dol:SatisfactionRelation
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:hasPart ;
                                  owl:someValuesFrom dol:Model
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:hasPart ;
                                  owl:someValuesFrom dol:Signature
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:hasPart ;
                                  owl:someValuesFrom dol:Sentence
                                ] ;
                
                skos:definition "metaframework mathematically formalising the notion of a logic"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/InterfaceSignature

dol:InterfaceSignature rdf:type owl:Class ;
                       
                       rdfs:label "interface signature"@en ;
                       
                       rdfs:subClassOf dol:Signature ;
                       
                       skos:definition "signature mediating between an OMS and a module of that OMS in the sense that it contains those non-logical symbols that the sentences of the module and the sentences of the OMS have in common."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Interpretation

dol:Interpretation rdf:type owl:Class ;
                   
                   rdfs:label "interpretation"@en ;
                   
                   owl:equivalentClass dol:View ;
                   
                   skos:definition "OMS mapping that postulates a specialization relation between two OMS along a morphism between their signatures"@en ;
                   
                   skos:altLabel "refinement"@en ,
                                 "view"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Language

dol:Language rdf:type owl:Class ;
             
             rdfs:label "language"@en ;
             
             rdfs:subClassOf dol:LinguisticEntity ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty dol:hasPart ;
                               owl:someValuesFrom dol:CollectionOfExpressions
                             ] ;
             
             skos:note "collection is used here in the sense of syntactic collection"@en ;
             
             skos:definition "collection of expressions"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LanguageAspect

dol:LanguageAspect rdf:type owl:Class ;
                   
                   rdfs:label "language aspect"@en ;
                   
                   rdfs:subClassOf dol:LinguisticEntity ;
                   
                   skos:definition "set of language constructs of a given language, not necessarily forming a sublanguage"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LanguageGraph

dol:LanguageGraph rdf:type owl:Class ;
                  
                  rdfs:label "OMS language graph"@en ;
                  
                  rdfs:subClassOf dol:Graph ;
                  
                  skos:definition "graph of OMS languages and OMS language translations, typically used in a heterogeneous environment."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LanguageMapping

dol:LanguageMapping rdf:type owl:Class ;
                    
                    rdfs:label "language mapping"@en ;
                    
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty dol:mapsFrom ;
                                                                 owl:onClass dol:Language ;
                                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty dol:mapsTo ;
                                                                 owl:onClass dol:Language ;
                                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                               ]
                                                             )
                                        ] ;
                    
                    rdfs:subClassOf dol:Mapping ;
                    
                    skos:definition "mapping between languages"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LinguisticEntity

dol:LinguisticEntity rdf:type owl:Class ;
                     
                     rdfs:label "linguistic entity"@en ;
                     
                     skos:definition "entity that is of linguistic, syntactic nature."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LinkedData

dol:LinkedData rdf:type owl:Class ;
               
               rdfs:label "linked data"@en ;
               
               rdfs:subClassOf dol:WebEntity ;
               
               skos:note " RDF, serialized as RDF/XML \\cite{W3C:REC-rdf-syntax-grammar-20040210}, is the most common format for publishing linked data. However, its usage is not mandatory. "@en ,
                         " The linked data principles (adapted from \\cite{BernersLee:LinkedData2006} and its paraphrase at \\cite{Wikipedia:LinkedData2011}) are the following: \\begin{enumerate} \\item Use IRIs as names for things. \\item Use HTTP IRIs so that these things can be referred to and looked up (``dereferenced'') by people and user agents.\\footnote{I.e., the IRI is treated as a URL (uniform resource locator).} \\item Provide useful machine-processable (plus optionally human-readable) information about the thing when its IRI is dereferenced, using standard formats. \\item Include links to other, related IRIs in the exposed data to improve discovery of other related information on the Web. \\end{enumerate} "@en ,
                         " Using HTTP content negotiation \\cite{rfc2616} it is possible to serve representations in different formats from the same URL. "@en ;
               
               skos:definition "structured data that is published on the Web in a machine-processable way, according to principles explained in http://linkeddata.org/"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LocalEnvironment

dol:LocalEnvironment rdf:type owl:Class ;
                     
                     rdfs:label "local environment"@en ;
                     
                     rdfs:subClassOf dol:Signature ;
                     
                     skos:definition "signature built from all previously-declared symbols and axioms."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Logic

dol:Logic rdf:type owl:Class ;
          
          rdfs:label "logic"@en ;
          
          rdfs:subClassOf dol:MathematicalEntity ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty dol:isFormalizedBy ;
                            owl:someValuesFrom dol:Institution
                          ] ;
          
          skos:example " $\\mathcal{SROIQ}(D)$ is the logic underlying OWL 2 DL. "@en ;
          
          skos:note " Most OMS languages have an underlying logic. "@en ,
                    " See annex~\\ref{a:rdf-logic-vocab} for the organization of the relation between OMS languages and their logics and serializations. "@en ;
          
          rdfs:comment "a logic that defines the semantics of an language"@en ;
          
          skos:definition "specification of valid reasoning that comprises signatures, sentences, models, and a satisfaction relation between models and sentences"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LogicGraph

dol:LogicGraph rdf:type owl:Class ;
               
               rdfs:label "logic graph"@en ;
               
               rdfs:subClassOf dol:Graph ;
               
               skos:note "In a logic graph, some of the logic translations and reductions can be marked to be default translations."@en ;
               
               skos:definition "graph of logics, logic translations and logic reductions, typically used in a heterogeneous environment."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LogicMapping

dol:LogicMapping rdf:type owl:Class ;
                 
                 rdfs:label "logic mapping"@en ;
                 
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                              owl:onProperty dol:mapsFrom ;
                                                              owl:onClass dol:Logic ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty dol:mapsTo ;
                                                              owl:onClass dol:Logic ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                            ]
                                                          )
                                     ] ;
                 
                 rdfs:subClassOf dol:Mapping ,
                                 [ rdf:type owl:Class ;
                                   owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty dol:mapsFrom ;
                                                          owl:onClass dol:Logic ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty dol:mapsTo ;
                                                          owl:onClass dol:Logic ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                        ]
                                                      )
                                 ] ;
                 
                 dol:propagatesToAdjoint "true"^^xsd:boolean ;
                 
                 rdfs:comment "a mapping (translation or reduction) between two logics"@en ;
                 
                 skos:definition "mapping between logics"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LogicReduction

dol:LogicReduction rdf:type owl:Class ;
                   
                   rdfs:label "logic reduction"@en ;
                   
                   rdfs:subClassOf dol:LogicMapping ;
                   
                   skos:note "mapping between logics forgetting parts of the structure, projection to a smaller logic, in contrast to reduction"@en ;
                   
                   skos:definition "reduction of a source logic onto a (usually less expressive) target logic (mapping signatures, sentences and models) that simply forgets those parts of the logical structure not fitting the target logic"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LogicTranslation

dol:LogicTranslation rdf:type owl:Class ;
                     
                     rdfs:label "logic translation"@en ;
                     
                     rdfs:subClassOf dol:LogicMapping ;
                     
                     skos:note "mapping between logics representing all structure, in contrast to reduction"@en ;
                     
                     skos:definition "translation of a source logic into a target logic (mapping signatures, sentences and models) that keeps or encodes the logical content of OMS"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LogicalLanguageAspect

dol:LogicalLanguageAspect rdf:type owl:Class ;
                          
                          rdfs:label "logical language aspect"@en ;
                          
                          rdfs:subClassOf dol:LanguageAspect ;
                          
                          skos:definition "the (unique) language aspect of an OMS language that enables the expression of non-logical symbols and sentences in a logical language"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/LogicalTheory

dol:LogicalTheory rdf:type owl:Class ;
                  
                  rdfs:label "logical theory"@en ;
                  
                  rdfs:subClassOf dol:MathematicalEntity ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty dol:hasPart ;
                                    owl:onClass dol:SetOfSentences ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty dol:hasPart ;
                                    owl:onClass dol:Signature ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                  ] ;
                  
                  skos:note "The sentences must use only those non-logical symbols that are present in the signature."@en ;
                  
                  skos:definition "signature equipped with a set of sentences over the signature"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/MDEModel

dol:MDEModel rdf:type owl:Class ;
             
             rdfs:label "MDE model"@en ;
             
             rdfs:subClassOf dol:OMS ;
             
             skos:note """Not to be confused with the term \\termref{model} in the sense
of logic (model theory)."""@en ;
             
             skos:definition "logical theory that is used as a model in the sense of model-driven engineering (MDE)"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Mapping

dol:Mapping rdf:type owl:Class ;
            
            rdfs:label "mapping"@en ;
            
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty dol:mapsFrom ;
                                                         owl:cardinality "1"^^xsd:nonNegativeInteger
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty dol:mapsTo ;
                                                         owl:cardinality "1"^^xsd:nonNegativeInteger
                                                       ]
                                                     )
                                ] ;
            
            rdfs:subClassOf dol:MathematicalEntity ;
            
            rdfs:comment "a generic mapping (translation or reduction)"@en ;
            
            skos:note "alternative term: function"@en ,
                      "in some cases is a morphism, as in Category Theory."@en ;
            
            skos:definition "relation between a set of inputs and a set of permissible outputs with the property that each input is related to exactly one output."@en ;
            
            dol:reifies dol:isMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/Matcher

dol:Matcher rdf:type owl:Class ;
            
            rdfs:label "matcher"@en ;
            
            rdfs:subClassOf dol:Tool ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty dol:hasProficiency ;
                              owl:someValuesFrom dol:Matching
                            ] ;
            
            skos:definition "tool that implements matching"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Matching

dol:Matching rdf:type owl:Class ;
             
             rdfs:label "matching"@en ;
             
             rdfs:subClassOf dol:Process ;
             
             skos:definition "algorithmic procedure that generates an alignment for two given OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity

dol:MathematicalEntity rdf:type owl:Class ;
                       
                       rdfs:label "mathematical entity"@en ;
                       
                       skos:definition "entity that is part of a mathematical theory."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Maximization

dol:Maximization rdf:type owl:Class ;
                 
                 rdfs:label "maximization"@en ;
                 
                 rdfs:subClassOf dol:Closure ;
                 
                 skos:definition "form of closure that restricts the models to those that are maximal (with respect to the local environment)."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/MaximumApproximant

dol:MaximumApproximant rdf:type owl:Class ;
                       
                       rdfs:label "maximum approximant"@en ;
                       
                       rdfs:subClassOf dol:Approximant ;
                       
                       skos:note "Technically, a maximum approximant is a uniform interpolant"@en ;
                       
                       skos:definition "best possible (in the sense of a maximum set of logical consequences approximant of an OMS in a smaller signature or a sublanguage."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Minimization

dol:Minimization rdf:type owl:Class ;
                 
                 rdfs:label "minimization"@en ;
                 
                 rdfs:subClassOf dol:Closure ;
                 
                 skos:note "Also known as circumscription."@en ;
                 
                 skos:definition "form of closure that restricts the models to those that are minimal (with respect to the local environment)."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Model

dol:Model rdf:type owl:Class ;
          
          rdfs:label "model"@en ;
          
          rdfs:subClassOf dol:MathematicalEntity ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty dol:isModelOfSignature ;
                            owl:someValuesFrom dol:Signature
                          ] ;
          
          skos:definition " semantic interpretation of all non-logical symbols of a signature"@en ;
          
          skos:note "A model of an OMS is a model of the signature of the OMS that also satisfies all the additional constraints expressed by the OMS. In case of flattenable OMS, these constraints are expressed by the axioms of the OMS."@en ;
          
          skos:scopeNote "This term is not to be confused with model (here called MDEModel) in the sense of modeling (i.e., the ``M'' in OMS)."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ModelBijectiveMapping

dol:ModelBijectiveMapping rdf:type owl:Class ;
                          
                          rdfs:label "model bijective mapping"@en ;
                          
                          rdfs:subClassOf dol:ModelExpansiveMapping ,
                                          dol:WeaklyExactMapping ;
                          
                          dol:propagatesToAdjoint "true"^^xsd:boolean ;
                          
                          skos:definition "logic mapping that has a bijective mapping of models"@en ;
                          
                          dol:reifies dol:isMappableToBijectivelyOnModels .



###  http://www.omg.org/spec/DOL/DOL-terms/ModelExpansiveMapping

dol:ModelExpansiveMapping rdf:type owl:Class ;
                          
                          rdfs:label "model expansive mapping"@en ;
                          
                          rdfs:subClassOf dol:FaithfulMapping ;
                          
                          dol:propagatesToAdjoint "true"^^xsd:boolean ;
                          
                          skos:definition "logic mapping that has a surjective model translation (ensuring faithfulness of the mapping)"@en ;
                          
                          dol:reifies dol:isModelExpansivelyMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/ModelFinder

dol:ModelFinder rdf:type owl:Class ;
                
                rdfs:label "model finder"@en ;
                
                rdfs:subClassOf dol:Tool ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:hasProficiency ;
                                  owl:someValuesFrom dol:ModelFinding
                                ] ;
                
                skos:definition "Tool that implements model finding"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ModelFinding

dol:ModelFinding rdf:type owl:Class ;
                 
                 rdfs:label "model finding"@en ;
                 
                 rdfs:subClassOf dol:Process ;
                 
                 skos:definition "process that finds models of an OMS and thus proves it to be satisfiable"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ModelTheoreticConservativeExtension

dol:ModelTheoreticConservativeExtension rdf:type owl:Class ;
                                        
                                        rdfs:label "model-theoretic conservative extension"@en ;
                                        
                                        rdfs:subClassOf dol:ConservativeExtension ;
                                        
                                        skos:note "An extension $O_2$ of an OMS $O_1$ is a model-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$."@en ;
                                        
                                        skos:definition "extension that does not lead to a restriction of class of models of an OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ModelTheoreticConservativeExtensionMapping

dol:ModelTheoreticConservativeExtensionMapping rdf:type owl:Class ;
                                               
                                               rdfs:label "model-theoretic conservative extension mapping"@en ;
                                               
                                               rdfs:subClassOf dol:ConservativeExtensionMapping ;
                                               
                                               skos:note "An extension $O_2$ of an OMS $O_1$ is a model-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$."@en ,
                                                         "Any model-theoretic conservative extension is also a consequence-theoretic one."@en ;
                                               
                                               skos:definition "extension mapping that does not lead to a restriction of class of models of an OMS"@en ;
                                               
                                               dol:reifies dol:isModelTheoreticallyConservativeExtensionOf .



###  http://www.omg.org/spec/DOL/DOL-terms/Module

dol:Module rdf:type owl:Class ;
           
           rdfs:label "module"@en ;
           
           rdfs:subClassOf dol:DOLStructuredOMS ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty dol:hasPart ;
                             owl:someValuesFrom dol:InterfaceSignature
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty dol:hasPart ;
                             owl:someValuesFrom dol:DOLStructuredOMS
                           ] ;
           
           skos:definition "structured OMS expressing a subOMS that conservatively extends to conservative extension the whole OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ModuleExtraction

dol:ModuleExtraction rdf:type owl:Class ;
                     
                     rdfs:label "module extraction"@en ;
                     
                     rdfs:subClassOf dol:Process ;
                     
                     sm:informativeReference "Cited and slightly adapted from \\cite{SuarezFigueroaEtAl:OntologyGlossary2008}"@en ;
                     
                     skos:note "The goal of module extraction is ``decomposing an OMS into smaller, more manageable modules with appropriate dependencies'' \\cite{DBLP:series/lncs/5445}"@en ;
                     
                     skos:definition "activity of obtaining from an OMS concrete modules to be used for a particular purpose (e.g. to contain a particular sub-signature of the original OMS)"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/ModuleExtractor

dol:ModuleExtractor rdf:type owl:Class ;
                    
                    rdfs:label "module extractor"@en ;
                    
                    rdfs:subClassOf dol:Tool ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty dol:hasProficiency ;
                                      owl:someValuesFrom dol:ModuleExtraction
                                    ] ;
                    
                    skos:definition "tool that implements module extraction"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/MonomorphicExtension

dol:MonomorphicExtension rdf:type owl:Class ;
                         
                         rdfs:label "monomorphic extension"@en ;
                         
                         rdfs:subClassOf dol:ModelTheoreticConservativeExtension ;
                         
                         skos:note "An extension $O_2$ of an OMS $O_1$ is a monomorphic extension, if each model of $O_1$ can be expanded to a model of $O_2$ that is unique up to isomorphism."@en ;
                         
                         skos:definition "extension whose newly introduced non-logical symbols are interpreted in a way unique up to isomorphism."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/MonomorphicExtensionMapping

dol:MonomorphicExtensionMapping rdf:type owl:Class ;
                                
                                rdfs:label "monomorphic extension mapping"@en ;
                                
                                rdfs:subClassOf dol:ModelTheoreticConservativeExtensionMapping ;
                                
                                skos:note "An extension $O_2$ of an OMS $O_1$ is a monomorphic extension, if each model of $O_1$ can be expanded to a model of $O_2$ that is unique up to isomorphism."@en ,
                                          "Each monomorphic extension is also a model-theoretic conservative extension but not vice versa."@en ;
                                
                                skos:definition "extension whose newly introduced non-logical symbols are interpreted in a way unique up to isomorphism"@en ;
                                
                                dol:reifies dol:isMonomorphicExtensionOf .



###  http://www.omg.org/spec/DOL/DOL-terms/NativeDocument

dol:NativeDocument rdf:type owl:Class ;
                   
                   rdfs:label "native document"@en ;
                   
                   rdfs:subClassOf dol:Document ;
                   
                   skos:definition "document containing a native OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/NativeOMS

dol:NativeOMS rdf:type owl:Class ;
              
              rdfs:label "native OMS"@en ;
              
              rdfs:subClassOf dol:OMS ;
              
              skos:definition "sequence of expressions (like non-logical symbols, sentences and structuring elements) from a given OMS language."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/NonLogicalSymbol

dol:NonLogicalSymbol rdf:type owl:Class ;
                     
                     rdfs:label "non-logical symbol"@en ;
                     
                     rdfs:subClassOf dol:Term ;
                     
                     skos:example " Non-logical symbols in Common Logic \\nisref{ISO/IEC 24707:2007} comprise \\begin{itemize} \\item names (denoting objects from the domain of discourse), \\item sequence markers (denoting sequences of objects). \\end{itemize} This is opposed to logical symbols in Common Logic, e.g.\\ logical connectives and quantifiers."@en ,
                                  "Non-logical symbols in OWL \\nisref{W3C/TR REC-owl2-syntax:2009} (there called ``entities'') comprise \\begin{itemize} \\item individuals (denoting objects from the domain of discourse), \\item classes (denoting sets of objects; also called concepts), and \\item properties (denoting binary relations over objects; also called roles). \\end{itemize} This is opposed to logical symbols in OWL, e.g.\\ those for intersection and union of classes. "@en ;
                     
                     skos:altLabel "OMS symbol"@en ;
                     
                     skos:note "The notion of ``atomic sentence'' used in logic is different, it usually may involve several non-logical symbols. "@en ;
                     
                     rdfs:comment "a non-logical symbol in an OMS"@en ;
                     
                     skos:definition "atomic expression or syntactic constituent of an OMS that requires an interpretation through a model"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/OMS

dol:OMS rdf:type owl:Class ;
        
        rdfs:label "OMS (ontology, specification or MDE model)"@en ;
        
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( dol:HeterogeneousOMS
                                            dol:HomogeneousOMS
                                          )
                            ] ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( dol:ElusiveOMS
                                            dol:FlattenableOMS
                                          )
                            ] ;
        
        rdfs:subClassOf dol:DOLEntity ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty dol:semanticallyDenotes ;
                          owl:someValuesFrom dol:OMSSemanticObject
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty dol:hasSentences ;
                          owl:someValuesFrom dol:SetOfSentences
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty dol:hasModels ;
                          owl:someValuesFrom dol:ClassOfModels
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty dol:hasLogic ;
                          owl:someValuesFrom dol:Logic
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty dol:hasSignature ;
                          owl:someValuesFrom dol:Signature
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty dol:usesLanguage ;
                          owl:someValuesFrom dol:OMSLanguage
                        ] ;
        
        skos:note "An OMS can be written in different OMS language serializations."@en ,
                  "An OMS has a single signature and model class over that signature as its model-theoretic semantics."@en ,
                  "An OMS is a collection of expressions (like non-logical symbols, sentences and structuring elements) in a given OMS language (or several such languages)"@en ;
        
        skos:definition "a basic or structured ontology, specification or MDE model"@en ;
        
        skos:note "collection is used here in the sense of syntactic collection"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/OMSDocumentation

dol:OMSDocumentation rdf:type owl:Class ;
                     
                     rdfs:label "OMS documentation"@en ;
                     
                     rdfs:subClassOf dol:LinguisticEntity ;
                     
                     sm:informativeReference "Adapted from \\cite{SuarezFigueroaEtAl:OntologyGlossary2008}"@en ;
                     
                     skos:definition "set of all annotations to an OMS, plus any other documents and explanatory comments generated during the entire OMS building process"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/OMSLanguage

dol:OMSLanguage rdf:type owl:Class ;
                
                rdfs:label "OMS language"@en ;
                
                rdfs:subClassOf dol:Language ,
                                [ rdf:type owl:Class ;
                                  owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty dol:supportsLogic ;
                                                         owl:someValuesFrom dol:Logic
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty dol:supportsSerialization ;
                                                         owl:someValuesFrom dol:Serialization
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty dol:supportsLogic ;
                                                         owl:allValuesFrom dol:Logic
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty dol:supportsSerialization ;
                                                         owl:allValuesFrom dol:Serialization
                                                       ]
                                                     )
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dol:supportsLogic ;
                                  owl:someValuesFrom dol:Logic
                                ] ;
                
                skos:note " An OMS language is used for the formal specification of OMS."@en ;
                
                skos:example " OMS languages include OWL, Common Logic, F-logic, UML class diagrams, RDFS, and OBO. "@en ;
                
                rdfs:comment "a language for OMS (ontologies, models or specifications)"@en ;
                
                skos:definition "language equipped with a formal, declarative, logic-based semantics, plus non-logical annotations"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/OMSLanguageTranslation

dol:OMSLanguageTranslation rdf:type owl:Class ;
                           
                           rdfs:label "OMS language translation"@en ;
                           
                           rdfs:subClassOf dol:LanguageMapping ,
                                           dol:Mapping ,
                                           [ rdf:type owl:Class ;
                                             owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                    owl:onProperty dol:mapsFrom ;
                                                                    owl:onClass dol:OMSLanguage ;
                                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty dol:mapsTo ;
                                                                    owl:onClass dol:OMSLanguage ;
                                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                                  ]
                                                                )
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty dol:mapsTo ;
                                             owl:onClass dol:OMSLanguage ;
                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty dol:mapsFrom ;
                                             owl:onClass dol:OMSLanguage ;
                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                           ] ;
                           
                           dol:propagatesToAdjoint "true"^^xsd:boolean ;
                           
                           skos:note " An OMS language translation shall satisfy the property that the result of a translation is a well-formed text in the target language. "@en ;
                           
                           skos:definition "a mapping (translation or reduction) between two OMS languages"@en ,
                                           "mapping from constructs in the source OMS language to their equivalents in the target OMS language"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/OMSMapping

dol:OMSMapping rdf:type owl:Class ;
               
               rdfs:label "OMS mapping"@en ;
               
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty dol:mapsFrom ;
                                                            owl:onClass dol:OMS ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty dol:mapsTo ;
                                                            owl:onClass dol:OMS ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                          ]
                                                        )
                                   ] ;
               
               rdfs:subClassOf dol:DOLEntity ;
               
               rdfs:comment "a mapping between two heterogeneous OMS in a DOL library"@en ;
               
               skos:definition "relationship between two OMS, typically given as a set of correspondences"@en ;
               
               dol:reifies dol:isMappableTo .



###  http://www.omg.org/spec/DOL/DOL-terms/OMSNetwork

dol:OMSNetwork rdf:type owl:Class ;
               
               rdfs:label "OMS network"@en ;
               
               rdfs:subClassOf dol:DOLEntity ;
               
               skos:note "An OMS network is a diagram of OMS in the sense of category theory, but different from a diagram in the sense of model-driven architecture."@en ,
                         "collection is used here in the sense of syntactic collection"@en ;
               
               skos:definition "collection of named OMS, possibly written in different OMS languages, linked by named OMS mappings and named OMS networks"@en ;
               
               skos:altLabel "hyperontology"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/OMSReduction

dol:OMSReduction rdf:type owl:Class ;
                 
                 rdfs:label "OMS reduction"@en ;
                 
                 rdfs:subClassOf dol:DOLStructuredOMS ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty dol:hasPart ;
                                   owl:someValuesFrom dol:DOLStructuredOMS
                                 ] ;
                 
                 owl:disjointWith dol:OMSTranslation ;
                 
                 skos:definition "DOL structured OMS expressing the restriction of an OMS to a smaller signature."@en ;
                 
                 dol:reifies dol:isReducibleTo .



###  http://www.omg.org/spec/DOL/DOL-terms/OMSSemanticObject

dol:OMSSemanticObject rdf:type owl:Class ;
                      
                      rdfs:label "OMS Semantic Object"@en ;
                      
                      rdfs:subClassOf dol:SemanticEntity ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty dol:hasSignature ;
                                        owl:someValuesFrom dol:Signature
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty dol:hasModels ;
                                        owl:someValuesFrom dol:ClassOfModels
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty dol:hasSentences ;
                                        owl:someValuesFrom dol:SetOfSentences
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty dol:hasLogic ;
                                        owl:someValuesFrom dol:Logic
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty dol:isMemberOf ;
                                        owl:someValuesFrom dol:SemanticDomain
                                      ] ;
                      
                      skos:definition "object giving the semantics of an OMS"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/OMSTranslation

dol:OMSTranslation rdf:type owl:Class ;
                   
                   rdfs:label "OMS translation"@en ;
                   
                   rdfs:subClassOf dol:DOLStructuredOMS ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty dol:hasPart ;
                                     owl:someValuesFrom dol:DOLStructuredOMS
                                   ] ;
                   
                   skos:note " An OMS translation results in an OMS mapping between the original and the translation OMS. "@en ;
                   
                   skos:definition "DOL structured OMS expressing the assignment of new names to some non-logical symbols of an OMS, or translation of an OMS along a language translation"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Ontology

dol:Ontology rdf:type owl:Class ;
             
             rdfs:label "ontology"@en ;
             
             rdfs:subClassOf dol:OMS ;
             
             skos:definition "logical theory that is used as a shard conceptualization"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/PlainMapping

dol:PlainMapping rdf:type owl:Class ;
                 
                 rdfs:label "plain mapping"@en ;
                 
                 rdfs:subClassOf dol:LogicMapping ;
                 
                 owl:disjointWith dol:SimpleTheoroidalMapping ;
                 
                 dol:propagatesToAdjoint "true"^^xsd:boolean ;
                 
                 rdfs:comment "Developers' note: Protégé 4.2 doesn't like 'Class: OMSMapping DisjointUnionOf: PlainMapping, SimpleTheoroidalMapping', so we do it this way:"@en ;
                 
                 skos:definition "logic mapping that maps signatures to signatures and therefore does not use infrastructure axioms"@en ;
                 
                 dol:reifies dol:isPlainlyMappableTo .
[ rdf:type owl:Axiom ;
  rdfs:comment "Developers' note: Protégé 4.2 doesn't like 'Class: OMSMapping DisjointUnionOf: PlainMapping, SimpleTheoroidalMapping', so we do it this way:"@en ;
  owl:annotatedSource dol:PlainMapping ;
  owl:annotatedTarget dol:SimpleTheoroidalMapping ;
  owl:annotatedProperty owl:disjointWith
] .



###  http://www.omg.org/spec/DOL/DOL-terms/Process

dol:Process rdf:type owl:Class ;
            
            rdfs:label "process"@en ;
            
            skos:definition "algorithmic procedure working on DOL-related linguistic entities as input."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Profile

dol:Profile rdf:type owl:Class ;
            
            rdfs:label "profile"@en ;
            
            rdfs:subClassOf dol:OMSLanguage ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty dol:supportsLogic ;
                              owl:someValuesFrom dol:Logic
                            ] ;
            
            skos:note " Profiles can have different logics, even with completely different semantics, e.g.\\ OWL 2 DL versus OWL 2 Full."@en ,
                      " The logic needs to support the language. "@en ;
            
            skos:definition "(syntactic) sublanguage of an OMS language interpreting according to a particular logic that targets specific applications or reasoning methods} "@en ;
            
            skos:example "Profiles of OWL 2 include OWL 2 EL, OWL 2 QL, OWL 2 RL, OWL 2 DL, and OWL 2 Full."@en ;
            
            skos:note "Profiles typically correspond to sublogics. "@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Query

dol:Query rdf:type owl:Class ;
          
          rdfs:label "query"@en ;
          
          rdfs:subClassOf dol:Sentence ;
          
          skos:definition "sentence containing query variables that can be instantiated by a substitution"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/QueryLanguage

dol:QueryLanguage rdf:type owl:Class ;
                  
                  rdfs:label "query language"@en ;
                  
                  rdfs:subClassOf dol:OMSLanguage ;
                  
                  skos:example " SPARQL, Prolog "@en ;
                  
                  skos:scopeNote " There are also general purpose OMS languages, which can express both OMS and quries."@en ;
                  
                  skos:definition "OMS language specifically dedicated to queries"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/QueryVariable

dol:QueryVariable rdf:type owl:Class ;
                  
                  rdfs:label "query variable"@en ;
                  
                  rdfs:subClassOf dol:NonLogicalSymbol ;
                  
                  skos:note "From an abstract point of view, query variables are just symbols; they are used in a way that they will be substituted using a substitution. Many OMS languages have special notations for (query) variables."@en ,
                            "If there are no variables in an OMS language, constants can be used as query variables."@en ,
                            "Usually, query variables are the free variables of a sentence, whereas there can be other (bound) variables."@en ;
                  
                  skos:definition "symbol that will be used in a query and a substitution"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Refinement

dol:Refinement rdf:type owl:Class ;
               
               rdfs:label "refinement"@en ;
               
               rdfs:subClassOf dol:OMSMapping ;
               
               skos:definition "OMS mapping that postulates a specialization relation between two OMS along a morphism between their signatures"@en ;
               
               rdfs:comment "refinement"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Resource

dol:Resource rdf:type owl:Class ;
             
             rdfs:label "resource"@en ;
             
             rdfs:subClassOf dol:WebEntity ;
             
             skos:example "Familiar examples include an electronic document, an image, a source of information with a consistent purpose (e.g., ``today's weather report for Los Angeles''), a service (e.g., an HTTP-to-SMS gateway), and a collection of other resources. A resource is not necessarily accessible via the Internet; e.g., human beings, corporations, and bound books in a library can also be resources. Likewise, abstract concepts can be resources, such as the operators and operands of a mathematical equation, the types of a relationship (e.g., ``parent'' or ``employee''), or numeric values (e.g., zero, one, and infinity). \\nisref{IETF/RFC 3986:2005, Section 1.1}"@en ;
             
             skos:note "\\nisref{IETF/RFC 3986:2005, Section 1.1} deliberately defines a resource as ``in a general sense \\textelp{} whatever might be identified by \\textins*{an IRI}''. The original source refers to URIs, but DOL uses the compatible IRI standard \\nisref{IETF/RFC 3987:2005} for identification."@en ;
             
             skos:definition "something that can be globally identified"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/SatisfactionRelation

dol:SatisfactionRelation rdf:type owl:Class ;
                         
                         rdfs:label "satisfaction relation"@en ;
                         
                         rdfs:subClassOf dol:MathematicalEntity ;
                         
                         skos:definition "relation between models and sentences indicating which sentences hold true in the model"@en ;
                         
                         rdfs:seeAlso dol:satisfies .



###  http://www.omg.org/spec/DOL/DOL-terms/SatisfiableOMS

dol:SatisfiableOMS rdf:type owl:Class ;
                   
                   rdfs:label "satisfiable OMS"@en ;
                   
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:intersectionOf ( dol:OMS
                                                              [ rdf:type owl:Class ;
                                                                owl:complementOf dol:UnsatisfiableOMS
                                                              ]
                                                            )
                                       ] ;
                   
                   rdfs:subClassOf dol:ConsistentOMS ;
                   
                   skos:definition "OMS that is satisfied by least one model."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/SemanticDomain

dol:SemanticDomain rdf:type owl:Class ;
                   
                   rdfs:label "semantic domain"@en ;
                   
                   rdfs:subClassOf dol:SemanticEntity ,
                                   dol:Set ;
                   
                   skos:definition "mathematically-defined set of values that can represent the intended meanings of language constructs."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/SemanticEntity

dol:SemanticEntity rdf:type owl:Class ;
                   
                   rdfs:label "semantic entity"@en ;
                   
                   skos:definition "entity that is of semantic nature, i.e. relating linguistic objects with their semantic denotation in a mathematical structure."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/SemanticRule

dol:SemanticRule rdf:type owl:Class ;
                 
                 rdfs:label "semantic rule"@en ;
                 
                 rdfs:subClassOf dol:SemanticEntity ;
                 
                 skos:definition "specification of a mapping from abstract syntax to a semantic domain."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Sentence

dol:Sentence rdf:type owl:Class ;
             
             rdfs:label "sentence"@en ;
             
             rdfs:subClassOf dol:Term ;
             
             skos:note "A sentence can conform to one or more signatures (namely those signatures containing all non-logical symbols used in the sentence)."@en ,
                       "In a model, on the one hand, a sentence is always true or false. In an OMS, on the other hand, a sentence can have several logical statuses: it can be an axiom, if postulated to be true; a theorem, if proven from other axioms and theorems; a conjecture, if expecting to be proven from other axioms and theorems; or have another of many possible statuses."@en ,
                       "It is quite common that sentences are required to be closed (i.e., have no free variables). However, this depends on the OMS language at hand."@en ;
             
             rdfs:comment "a sentence in an OMS"@en ;
             
             skos:definition "term that is either true or false in a given model, i.e., which is assigned a truth value in this model."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/SequenceOfCorrespondences

dol:SequenceOfCorrespondences rdf:type owl:Class ;
                              
                              rdfs:label "sequence of correspondences"@en ;
                              
                              rdfs:subClassOf dol:DOLEntity ;
                              
                              skos:note "sequence is used here in the sense of syntactic sequence"@en ;
                              
                              skos:definition "sequence whose members are correspondences."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/SequenceOfOMS

dol:SequenceOfOMS rdf:type owl:Class ;
                  
                  rdfs:label "sequence of OMS"@en ;
                  
                  rdfs:subClassOf dol:DOLEntity ;
                  
                  skos:note "sequence is used here in the sense of syntactic sequence"@en ;
                  
                  skos:definition "sequence whose members are OMS."@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Serialization

dol:Serialization rdf:type owl:Class ;
                  
                  rdfs:label "OMS serialization"@en ;
                  
                  rdfs:subClassOf dol:LinguisticEntity ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty dol:serializes ;
                                    owl:someValuesFrom dol:OMSLanguage
                                  ] ;
                  
                  skos:example " Common Logic uses the term ``dialect''; the following are standard Common Logic dialects: Common Logic Interchange Format (CLIF), Conceptual Graph Interchange Format (GCIF), eXtended Common Logic Markup Language (XCL)."@en ,
                               "OWL uses the term 'serialization' the following are standard OWL serializations: OWL functional-style syntax, OWL/XML, OWL Manchester syntax, plus any standard serialization of RDF (e.g., RDF/XML, Turtle, ...). However, RDF/XML is the only one tools are required to implement. "@en ;
                  
                  skos:note "Serializations serve as standard formats for exchanging OMS between tools."@en ;
                  
                  rdfs:comment "a serialization of an language"@en ;
                  
                  skos:definition "specific syntactic encoding of a given OMS language"@en ;
                  
                  rdfs:comment "synonymous to 'concrete syntax'"@en .



###  http://www.omg.org/spec/DOL/DOL-terms/Set

dol:Set rdf:type owl:Class ;
        
        rdfs:label "set"@en ;
        
        rdfs:subClassOf dol:MathematicalEntity ;
        
        skos:definition "set whose members have a certain type, as specified by the type parameter."@en .



