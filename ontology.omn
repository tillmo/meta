        rdfs:seeAlso satisfies,
        skos:definition "relation between models and sentences indicating which sentences hold true in the model"^^xsd:string
    
    
Class: ConservativeExtensionMapping

    Annotations: 
        skos:definition "consequence-theoretic or model-theoretic conservative extension"^^xsd:string,
        rdfs:label "conservative extension mapping",
        skos:note "If used without qualification, the consequence-theoretic version is meant."^^xsd:string
    
    SubClassOf: 
        ExtensionMapping
    
    
    
Class: HeterogeneousEnvironment

    Annotations: 
        rdfs:label "heterogeneous environment",
        skos:definition "environment for the expression of homogeneous and heterogeneous OMS, comprising a logic graph, an OMS language graph and a supports relation"^^xsd:string
    
    SubClassOf: 
        hasPart some OMSLanguageGraph,
        hasPart some OMSLogicGraph,
        hasPart some SupportsRelation
    
    
Class: LanguageAspect

    Annotations: 
        skos:definition "set of language constructs of a given language, not necessarily forming a sublanguage"^^xsd:string,
        rdfs:label "language aspect"
    
    
    
Class: LogicApproximation

    Annotations: 
        rdfs:label "logic approximation",
        skos:definition "mapping of a source logic onto a (usually less expressive) target logic that tries to approximate the OMSs expressed in the source logic with means of the expressivity of the target logic"^^xsd:string,
        skos:note " A unique maximal approximation need not exist. "^^xsd:string
    
    SubClassOf: 
        LogicMapping
    
    
Class: Profile

    Annotations: 
        skos:note "Profiles typically correspond to sublogics. "^^xsd:string,
        skos:definition "(syntactic) sublanguage of an OMS language interpreting according to a particular logic that targets specific applications or reasoning methods} "^^xsd:string,
        skos:note " Profiles can have different logics, even with completely different semantics, e.g.\\ OWL 2 DL versus OWL 2 Full."^^xsd:string,
        skos:note " The logic needs to support the language. "^^xsd:string,
        skos:example "Profiles of OWL 2 include OWL 2 EL, OWL 2 QL, OWL 2 RL, OWL 2 DL, and OWL 2 Full."^^xsd:string,
        rdfs:label "profile"
    
    SubClassOf: 
        supportsLogic some Logic,
        OMSLanguage
    
    
Class: FocusedOMS

    Annotations: 
        skos:definition "TBD"^^xsd:string,
        rdfs:label "focused OMS"
    
    SubClassOf: 
        OMS
    
    
Class: ExtensionMapping

    Annotations: 
        skos:definition "mapping between two OMSs where the sets of non-logical symbols and sentences of the second OMS are supersets of those present in the first OMS"^^xsd:string,
        rdfs:label "extension mapping",
        skos:editorialNote "This definition differs from that in the OntoIOp glossary by focussing on the mapping rather than the codomain of the mapping."^^xsd:string
    
    SubClassOf: 
        LogicalOMSMapping
    
    
Class: OMSDocumentation

    Annotations: 
        skos:definition "set of all annotations to an OMS, plus any other documents and explanatory comments generated during the entire OMS building process"^^xsd:string,
        rdfs:label "OMS documentation",
        sm:informativeReference "  
    Adapted from \\cite{SuarezFigueroaEtAl:OntologyGlossary2008}
  "
    
    
Class: SupportsRelation

    Annotations: 
        rdfs:label "supports relation"
    
    
Class: SetOfAnnotationExpressions

    Annotations: 
        rdfs:label "set of annotation expressions",
        skos:definition "set whose members are annotation expressions."^^xsd:string
    
    SubClassOf: 
        SetOfExpressions
    
    
Class: ElusiveOMS

    Annotations: 
        skos:definition "OMS that is not flattenable"^^xsd:string,
        rdfs:label "elusive OMS"
    
    SubClassOf: 
        OMS
    
    DisjointWith: 
        FlattenableOMS
    
    
    
Class: OMSNetwork

    Annotations: 
        skos:definition "collection of named OMSs, possibly written in different OMS languages, linked by named OMS mappings and named OMS graphs"^^xsd:string,
        skos:altLabel "hyperontology"^^xsd:string,
        rdfs:label "OMS network"
    
    
Class: WeakDefinitionalExtensionMapping

    Annotations: 
        skos:note "An extension is definitional if and only if it is both weakly definitional and model-theoretically conservative."^^xsd:string,
        skos:definition "extension whose newly introduced non-logical symbols can be interpreted in at most one way"^^xsd:string,
        rdfs:label "weak definitional extension mapping",
        skos:note "An extension $O_2$ of an OMS $O_1$ is a weak definitional extension, if each model of $O_1$ can be expanded to at most one model of $O_2$."^^xsd:string
    
    SubClassOf: 
        ExtensionMapping
    
    
Class: TheoremSentence

    Annotations: 
        rdfs:label "theorem sentence",
        skos:definition "sentence that has been proven from other axioms and theorems"^^xsd:string
    
    SubClassOf: 
        Sentence
    
    
Class: Mapping

    Annotations: 
        reifies mappableTo,
        rdfs:comment "a generic mapping (translation or reduction)",   
        skos:definition "TBD"^^xsd:string,
        rdfs:label "mapping",
        skos:note "in some cases is a morphism, as in Category Theory."^^xsd:string
    EquivalentTo: 
        (mapsFrom exactly 1 owl:Thing)
         and (mapsTo exactly 1 owl:Thing)
    
    
Class: AnnotationExpressionLanguageAspect

    Annotations: 
        skos:definition "the (unique) language aspect of an OMS language that enables the expression of comments and annotations"^^xsd:string,
        rdfs:label "annotation expression language aspect"
    
    SubClassOf: 
        LanguageAspect
    
    DisjointWith: 
        LogicalLanguageAspect, StructuringLanguageAspect
    
    
Class: Term

    Annotations: 
        skos:definition "syntactic expression either consisting of a single non-logical symbol or recursively composed of other terms (a.k.a. its subterms)"^^xsd:string,
        rdfs:label "term"
    
    SubClassOf: 
        Expression
    
    
Class: ConsequenceTheoreticConservativeExtensionMappin

    Annotations: 
        skos:definition "extension that does not add new theorems (in terms of the unextended signature)"^^xsd:string,
        rdfs:label "consequence-theoretic conservative extension mapping",
        skos:note "An extension $O_2$ of an OMS $O_1$ is a consequence-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$."^^xsd:string
    
    SubClassOf: 
        ConservativeExtensionMapping
    
    
Class: Resource

    Annotations: 
        skos:example "Familiar examples include an electronic document, an image, a source of information with a consistent purpose (e.g., ``today's weather report for Los Angeles''), a service (e.g., an HTTP-to-SMS gateway), and a collection of other resources. A resource is not necessarily accessible via the Internet; e.g., human beings, corporations, and bound books in a library can also be resources. Likewise, abstract concepts can be resources, such as the operators and operands of a mathematical equation, the types of a relationship (e.g., ``parent'' or ``employee''), or numeric values (e.g., zero, one, and infinity). \\nisref{IETF/RFC 3986:2005, Section 1.1}"^^xsd:string,
        skos:definition "something that can be globally identified} "^^xsd:string,
        skos:note "\\nisref{IETF/RFC 3986:2005, Section 1.1} deliberately defines a resource as ``in a general sense \\textelp{} whatever might be identified by \\textins*{an IRI}''. The original source refers to URIs, but DOL uses the compatible IRI standard \\nisref{IETF/RFC 3987:2005} for identification."^^xsd:string,
        rdfs:label "resource"
    
    
Class: OMSLogicGraph

    Annotations: 
        skos:definition "TBD"^^xsd:string,
        rdfs:label "OMS logic graph"
    
    SubClassOf: 
        Graph
    
    
Class: Approximation

    Annotations: 
        skos:definition "logical OMS mapping to a logically implied theory (possibly after suitable translation) in a smaller signature or OMS language"^^xsd:string,
        skos:editorialNote "this definition is derived from the defintion of approximant in the spec"^^xsd:string,
        rdfs:label "approximation"
    
    SubClassOf: 
        LogicalOMSMapping
    
    
Class: LogicalOMSMapping

    Annotations: 
        skos:note " Logical OMS mappings are given as sets of correspondences, which are required to be \\emph{signature morphisms}. "^^xsd:string,
        rdfs:label "logical OMS mapping",
        skos:definition "OMS mapping that has a formal, logic-based semantics"^^xsd:string
    
    SubClassOf: 
        hasPart some SetOfCorrespondences,
        OMSMapping
    
    
    
Class: QueryLanguage

    Annotations: 
        skos:scopeNote " There are also general purpose OMS languages, which can express both OMS and quries. "^^xsd:string,
        skos:definition "OMS language specifically dedicated to queries"^^xsd:string,
        rdfs:label "query language",
        skos:example " SPARQL, Prolog "^^xsd:string
    
    SubClassOf: 
        OMSLanguage
    
    
Class: Query

    Annotations: 
        skos:definition "sentence containing query variables that can be instantiated by a substitution"^^xsd:string,
        rdfs:label "query"
    
    SubClassOf: 
        Sentence
    
    
Class: OMSLanguageGraph

    Annotations: 
        rdfs:label "OMS language graph",
        skos:definition "TBD"^^xsd:string
    
    SubClassOf: 
        Graph
    
    
Class: HeterogeneousOMS

    Annotations: 
        skos:definition "OMS whose parts are supported by different logics"^^xsd:string,
        rdfs:label "heterogeneous OMS"
    
    SubClassOf: 
        OMS
    
    DisjointWith: 
        HomogeneousOMS
    
    
    
Class: Equivalence

    Annotations: 
        skos:definition "logical OMS mapping ensuring that two OMSs share the same definable concepts} "^^xsd:string,
        rdfs:label "equivalence",
        skos:note " Two OMSs are equivalent if they have a common definitional extension. The OMSs may be written in different OMS languages. "^^xsd:string
    
    SubClassOf: 
        LogicalOMSMapping
    
    
    
Class: LinkedData

    Annotations: 
        skos:definition "structured data that is published on the Web in a machine-processable way, according to principles specified in \\cite{BernersLee:LinkedData2006,BizerHeath09}}"^^xsd:string,
        skos:note " The linked data principles (adapted from \\cite{BernersLee:LinkedData2006} and its paraphrase at \\cite{Wikipedia:LinkedData2011}) are the following: \\begin{enumerate} \\item Use IRIs as names for things. \\item Use HTTP IRIs so that these things can be referred to and looked up (``dereferenced'') by people and user agents.\\footnote{I.e., the IRI is treated as a URL (uniform resource locator).} \\item Provide useful machine-processable (plus optionally human-readable) information about the thing when its IRI is dereferenced, using standard formats. \\item Include links to other, related IRIs in the exposed data to improve discovery of other related information on the Web. \\end{enumerate} "^^xsd:string,
        rdfs:label "linked data",
        skos:note " RDF, serialized as RDF/XML \\cite{W3C:REC-rdf-syntax-grammar-20040210}, is the most common format for publishing linked data. However, its usage is not mandatory. "^^xsd:string,
        skos:note " Using HTTP content negotiation \\cite{rfc2616} it is possible to serve representations in different formats from the same URL. "^^xsd:string
    
    
    
Class: LogicTranslation

    Annotations: 
        rdfs:label "logic translation",
        skos:definition "mapping of a source logic into a target logic (mapping signatures, sentences and models) that keeps or encodes the logical content of OMSs"^^xsd:string
    
    SubClassOf: 
        LogicMapping
    
    
Class: Graph

    Annotations: 
        skos:definition "TBD"^^xsd:string,
        rdfs:label "graph",
        skos:altLabel "diagram"^^xsd:string
    
    
Class: SetOfExpressions

    Annotations: 
        rdfs:label "set of expressions",
        skos:definition "set whose members are expressions"^^xsd:string
    
    SubClassOf: 
        Set
    
    
Class: LogicReduction

    Annotations: 
        skos:definition "mapping of a source logic onto a (usually less expressive) target logic (mapping signatures, sentences and models) that simply forgets those parts of the logical structure not fitting the target logic"^^xsd:string,
        rdfs:label "logic reduction"
    
    SubClassOf: 
        LogicMapping
    
    
Class: Expression

    Annotations: 
        skos:definition "TBD see Class 'language'"^^xsd:string,
        rdfs:label "expression"
    
    SubClassOf: 
        usesLanguage exactly 1 Language
    
    
    
Class: AnnotationExpression

    Annotations: 
        skos:definition "additional information without a logical semantics that is attached to an element of an OMS"^^xsd:string,
        skos:note "\\label{note:annotation-rdf} Formally, an annotation is given as a $(\\text{subject}, \\text{predicate}, \\text{object})$ triple as defined by \\nisref{SOURCE: W3C/TR REC-rdf-concepts:2004, Section 6}. The subject of an annotation is an element of an OMS. The predicate is an RDF property defined in an external OMS and describes in what way the annotation object is related to the annotation subject. "^^xsd:string,
        skos:note " According to note~\\ref{note:annotation-rdf} it is possible to interpret annotations under an RDF semantics. ``Without a logical semantics'' in this definition means that annotations to an OMS are not considered sentences of that OMS. "^^xsd:string,
        rdfs:label "annotation expression"
    
    SubClassOf: 
        Expression
    
    
Class: Renaming

    Annotations: 
        rdfs:label "renaming",
        skos:definition "assignment of new names to some non-logical symbols of an OMS"^^xsd:string,
        skos:note " A renaming results in a logical OMS mapping between the original and the renamed OMS. "^^xsd:string
    
    SubClassOf: 
        LogicalOMSMapping
    
    
Individual: DOL

    Annotations: 
        skos:note " When viewed as an \\termref{OMS language}, DOL has OMSs as its non-logical symbols, and OMS mappings as its sentences."^^xsd:string,
        rdfs:label "DOL",
        skos:definition "language for formalizing distributed OMSs, whose syntax and semantics are specified in this \\IS} "^^xsd:string
    
    Types: 
        OMSLanguage
    
    
