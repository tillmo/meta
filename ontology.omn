        rdfs:label "model",
        skos:scopeNote "This term is not to be confused with model (here called MDEModel) in the sense of modeling (i.e., the ``M'' in OMS)."^^xsd:string,
        skos:definition " semantic interpretation of a signature"^^xsd:string
    
